/*footer  start*/
body.mistakesCollection footer {
  background-color: #878787;
  position: fixed;
  width: 100%;
  bottom: 0;
  height: 0.85rem;
  z-index: 10;
}
body.mistakesCollection footer p {
  color: #FFFFFF;
  margin: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 0.24rem;
  vertical-align: middle;
}
body.mistakesCollection footer p img,
body.mistakesCollection footer p span {
  vertical-align: middle;
}
body.mistakesCollection footer p img {
  margin: 0.05rem -0.05rem 0;
  width: 0.2rem;
}
body.mistakesCollection footer p span {
  overflow: hidden;
  white-space: nowrap;
  position: relative;
  text-overflow: ellipsis;
  height: 0.85rem;
  line-height: 0.85rem;
  display: inline-block;
  padding:0 0.2rem;
  background: url("../img/dropdown_white.png") no-repeat right center;
  background-position:right 0.03rem center;
  background-size: 0.2rem;
  background-color: #878787;
  width: 1.08rem;
}
body.mistakesCollection footer p span:first-of-type {
    left: 50%;
    margin-left: -0.64rem;
}
body.mistakesCollection footer p span:nth-of-type(3) {
  width: 1.3rem;
}
body.mistakesCollection footer p span:last-of-type {
  width: 1.65rem;
  background-position: right 0.1rem center;
  padding-right: 0.3rem;
}
body.mistakesCollection footer p span:last-of-type.active{
	background-position: right 0.1rem center;
}
body.mistakesCollection footer p span.active {
  color: #878787;
  background: url("../img/dropdown_black.png") no-repeat right center;
  background-position: right 0.03rem center;
  background-color: #FFFFFF;
  background-size: 0.2rem;
}
