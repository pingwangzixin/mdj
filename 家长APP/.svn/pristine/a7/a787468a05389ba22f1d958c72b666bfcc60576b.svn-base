body{background: #eee;}

.wrap{overflow-x: hidden;}
.wrap_inner{margin-bottom: 1.04rem;}
/*banner*/
.banner{width: 100%; height:2.5rem; background: url(../img/banner.png) no-repeat center; background-size: 100% 100%;}

.fixed_box{width: 100%; top: 0; left: 0; z-index: 1; background: #eee;}
/*头部*/
.header{font-size: 0.28rem; height: 0.82rem; background: #fff; overflow-y: hidden;} 
.header dl{text-align: center; height: 0.82rem;width:300%; line-height: 0.82rem;  }
.header dl dt{width: 1.65rem;}
.header dl dd{float: left; margin: 0 0.36rem;}
.header dl dd.active{color: #24bfff;}

/*切换*/
.switch{width: 100%; overflow-x: auto; margin: 0.2rem 0; top: 0; left: 0; z-index: 1;}
.switch_x{width: 520px; text-align: center; height: 0.6rem; line-height: 0.6rem; font-size: 0.28rem; background: #fff; }
.switch span{width: 130px; color: #666; height:0.6rem;}
.switch span.active{background: #24bfff; color: #fff;}

/*资源列表*/
.tab_box{/*margin-bottom: 0.84rem;*/}
.list_box{display: none;}
.list_box:first-child{display: block;}
.list{position: relative; background: #fff; margin-bottom: 0.2rem; transition: transform .2s;}
.tit{border-bottom: 1px solid #ddd; font-size: 0.28rem; height: 0.69rem; line-height: 0.69rem; }
.tit h3{padding: 0 0.2rem; display: flex;}
.list h3 span{display: inline-block; width: 4.2rem; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.state{padding-right: 0.2rem; color: #999;}
.state img{width: 0.29rem; height: 0.29rem; vertical-align: -0.04rem;}
.list_detail{font-size: 0.24rem; color: #666; padding: 0.22rem 0 0.16rem 0.2rem;}
.list_detail p{line-height: 0.47rem;}
.main_img{width: 1.15rem; height: 0.97rem; margin-right: 0.22rem;overflow: hidden;}
.main_img img{height: 0.97rem; margin: 0 auto;}
.list_deLeft{margin-right: 0.3rem;}
.list_deLeft span{display: inline-block; vertical-align: bottom; width: 1.1rem; white-space: nowrap; text-overflow: ellipsis; overflow: hidden;}
.to_left{position: absolute; top: 0; right: -4.79rem;}
.slide_img{width: 0.52rem; height: 2.05rem;}
.list_option{color: #fff; text-align: center;}
.open{width: 1.84rem; height: 2.05rem; background: #bdbdbd; border-right: 1px solid #fff;}
.to_up{background: #fca800;}
.open img{display: block; width: 0.34rem; height: 0.34rem; margin: 0.62rem auto 0.15rem;}
.delete{width: 1.08rem; height: 2.05rem; background: #ff4864;}
.delete img{display: block; width: 0.3rem; height: 0.32rem; margin: 0.62rem auto 0.18rem;}

/*收藏与分享*/
.to_leftShare{right: -2.93rem;}

/*底部筛选框*/
.bott{width: 100%; position: fixed; left: 0; bottom: 0; z-index: 104; background: #878787; color: #fff;}
.screen_box{width: 100%; height: 0.84rem; line-height: 0.84rem;}
.screen_box div{position: relative; float: left; padding-left: 0.2rem; background: url(../img/dropdown_white.png) no-repeat center; background-size: 0.2rem 0.16rem; display: none; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; padding-right: 0.33rem;}
.screen_box div:first-child{display: block; left: 50%; /*transform: translateX(-50%);*/ margin-left: -0.64rem;}
.screen_box div.active{color: #000; background: url(../img/dropdown_black.png) #fff no-repeat center; background-size: 0.2rem 0.16rem;}
.screen_box div.grade{width: 1.08rem; background-position-x: 1rem;  text-overflow: initial; padding-right: 0;}
.screen_box div.subject{width: 1.08rem; background-position-x: 1.35rem;}
.screen_box div.unit{width: 1.3rem; background-position-x: 1.58rem;}
.screen_box div.topic{width: 1.65rem; background-position-x: 1.8rem; /*padding-right: 0.39rem;*/}
.screen_box>img{float: left; width: 0.2rem; height: 0.19rem; padding-top: 0.34rem; display: none;}

/*底部弹框年级 科目*/
.bomb .con{width: 100%; left: 0; top: inherit; bottom: 0.82rem; -webkit-transform: translate(0,0); -ms-transform: translate(0,0); transform: translate(0,0); border-radius: 0; font-size: 0.28rem;}
.bomb .con ul{max-height: 6.6rem; overflow-y: auto; z-index: 105; padding: 0 0.2rem;}
.bomb .con ul li{float: left; line-height: 1.1rem; padding: 0 0.17rem;} 
/*底部弹框单元 题目*/
.bomb .con ol{max-height: 6.39rem; overflow-y: auto; z-index: 105;}
.bomb .con ol li{height: 1.05rem; line-height: 1.05rem; border-bottom: 1px solid #ccc; text-align: left; padding: 0 0.34rem; border-bottom: 1px solid #ccc;}
.bomb .con em {
	display: block;
    width: 100%;
    font-style: normal;
    padding: .25rem 0;
    color: red;
}

/*搜索资源模块*/
/*搜索历史*/
.searchInpWrap{padding:0.24rem 0.2rem 0.24rem; background: #fff;}
.searchInpWrap .searchInp{background:#ebebeb; padding-left:0.1rem;  border-radius: 0.11rem;  float:right}
.searchInpWrap span{float:left; /*margin-right:0.13rem;*/line-height: 0.69rem;}
.searchInp input{float:left; width:5.3rem; height:0.69rem; background:#EBEBEB; padding-left:0.15rem; font-size: 0.26rem; font-family:'microsoft yahei'; color:#999}
.searchInp button{float:right; height:0.69rem; padding-left:0.3rem; background:none; padding-right:0.3rem;}
.searchInp button img{width:0.3rem; height:0.3rem}
.searchInp input::-webkit-input-placeholder{color:#bbb; font-size: 0.26rem; font-family:'microsoft yahei';}
.searchWrap{padding:0 0.2rem 0.12rem 0.2rem; border-top:1px solid #ccc; overflow: hidden; }
.searchWrap .searchHistory{margin-top:0.3rem; overflow: hidden;}
.searchWrap .searchHistory h3{font-size:0.27rem; margin-bottom:0.22rem; }
.searchWrap .searchHistory ul li{float:left; padding:0 0.15rem; line-height:0.48rem; margin-right:0.24rem ; margin-bottom:0.2rem ; background:#ebebeb; border-radius: 0.1rem;}
.resetHistory{margin-top:0.21rem; text-align:center}
/*搜索条件为空*/
.searchNull .imgNull{padding-top:1.05rem; padding-bottom:0.48rem; text-align: center;}
.searchNull .imgNull img{width:2.85rem; height:4.56rem}
.searchNull .searchTitle {padding-left:0.22rem; font-size:0.28rem}
.searchNull .searchTitle em{margin-left:0.08rem; color:#24BFFF}
.searchNull p{text-align: center; margin-bottom:0.2rem; color:#666}
.searchNull .reverse span{float:left; padding:0 0.8rem; line-height:0.6rem; height:0.6rem; border-radius: 0.1rem; background:#24bfff; margin-left:50%; -webkit-transform: translateX(-50%); color:white;  }
.searchNull .reverse {text-align: center;}
/*.searchNull .reverse a:active{color:white}*/

/*可以搜索出*/
.searchContent{width:100%; overflow:hidden; overflow-x: hidden;}
.searchContent h4{padding-left:0.2rem; background:#eee; padding-top:0.2rem;}
.searchContent h4 i{color:#fe4763;}


/*删除弹框*/
.per_del{}
.per_del .con{width:5.92rem; height:2.51rem; background:white; border-radius: 0.12rem;}
.per_del p{line-height:1.44rem; font-size:0.28rem; color:#000}
.per_del button{width:1.38rem; height:0.62rem; background:#24bfff; color:white; font-family: "microsoft yahei"; font-size:0.26rem; border-radius:0.12rem}
.per_del button:last-child{margin-left:0.45rem; background:#d9d9d9}


/*正在加载*/
/*.bottoLoading{position: fixed; width: 100%; bottom: 0.84rem; left: 0;}*/

/*无资源*/
.allNoresult{background: #eee;}
.allNoresult>img{margin:1.20rem auto 0;width:3.47rem;display:block;}
.allNoresult>p{padding-top:0.42rem;text-align:center;font-size:0.3rem;line-height:0.3rem;color:#999;}

/*共享弹框*/
.share_box .con{width:5.92rem; height:2.51rem; background:white; border-radius: 0.12rem;}
.share_box img{width: 1.03rem; height: 1.03rem; margin-top: 0.5rem;}
.share_box p{line-height: 0.78rem;}
