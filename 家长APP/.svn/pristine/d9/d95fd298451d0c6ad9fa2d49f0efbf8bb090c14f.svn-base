    $(function() {
			//判断版本
			var Num=100;
			var userAgent = navigator.userAgent;
			var index = userAgent.indexOf("Android")
			if(index >= 0) {
				var androidVersion = parseFloat(userAgent.slice(index + 8));
				if(androidVersion <= 4.4) {
					Num = 10;
				} else {
					Num = 100;
				}
			}
			
			//刷新加载
			var startY, endY;
			var scrollTopVal = 0;
			/*
		    document.getElementById("pullLoad").addEventListener("touchstart", touchStart, false);
		    document.getElementById("pullLoad").addEventListener("touchmove", touchMove, false);
		    document.getElementById("pullLoad").addEventListener("touchend", touchEnd, false); 
		    */
		    $('#pullLoad').on('touchstart',touchStart);
	        $('#pullLoad').on('touchmove',touchMove);
	        $('#pullLoad').on('touchend',touchEnd);
		    
		    function touchStart(event){
		        var touch = event.originalEvent.targetTouches[0];
		        startY = touch.pageY;
		    }   
		    
		    var moveStatus = 0;
		    function touchMove(event){
			    var touch = event.originalEvent.targetTouches[0];
			    endY = (startY-touch.pageY);
		        if(endY>=Num && moveStatus==0){
		        	startY = touch.pageY
		        	moveStatus=1;
		        	//注意  ：  当前节点的高度
		        	//滑动的低端
		            var totalheight = parseFloat($(document).height()) + parseFloat($(window).scrollTop());
		            if($(document).height()-75 <= totalheight){
		                $(".bottoLoading").show();
		                //ajax
		                /*
		                $.ajax({
		                    url:"date.json",
		                    type:'get',
		                    success:function(date){
		                        $(".bottoLoading").show();
		                    },
		                    error:function(){
		                 		$(".bottoLoading").show();
		                    }
		                });
		                */
		                
		                //模拟效果    可刪除。。。
		              setTimeout(function(){
		              	$(".bottoLoading").hide();
		              	moveStatus=0;
		              },1000);
		            }
		            
		        }else if(endY<=-Num&&moveStatus==0){
		        	startY = touch.pageY
		        	moveStatus=1;
		        	if(parseFloat($(window).scrollTop())===0){
		        		$('.topLoading').show();
//		        		$('.tabcon').css('margin-top',0);

		        	}
		        	setTimeout(function(){
		              	$(".topLoading").hide();
//		              	$('.tabcon').css('margin-top',1+'rem');

		              	moveStatus=0;
		            },1000);
		            
		        }
		    }
		    function touchEnd(event){
		    	moveStatus=0;
		    }
	});
    

</script>
<script type="text/javascript">
        //调用手机日历插件
        window.onload=function(){

            (function(mui) {
                mui.init();
                $(document).on("tap","#startTime",function(){
                    var getTime= document.querySelector('#endTime').innerText;
                    var endToArr=getTime.replace(/[\u4E00-\u9FA5]+/g,"-").replace(/-$/,'').split('-');
                    var endArr=endToArr;
                    var year=Number(endArr[0]);
                    var month=Number((endArr[1]-1));
                    var day=Number(endArr[2]);
                    var dateTime=(endToArr.length!==1)?new Date(year,month,day):new Date();
                    var picker = new mui.DtPicker({"type": "date","endDate": dateTime}) ;
                    picker.show(function(rs){
                        if(endToArr.length===1){
                            document.querySelector("#startTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                        }else{
                            var dateStart=new Date(endToArr[0],(endToArr[1]-1),endToArr[2]);
                            var dateEnd=new Date(rs.y.value,(rs.m.value-1),rs.d.value);
                            if(dateStart<dateEnd){
                                document.querySelector("#startTime").innerText='不合理时间';
                                $("#startTime").css('color','red');
                            }else{
                                document.querySelector("#startTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                                $("#startTime").css('color','#666');
                            }
                        }
                        picker.dispose();
                    });
                });
                $(document).on("tap","#endTime",function(){
                    var getTime= document.querySelector('#startTime').innerText;
                    var endToArr=getTime.replace(/[\u4E00-\u9FA5]+/g,"-").replace(/-$/,'').split('-');
                    var endArr=(endToArr.length!==1)?endToArr:[2006,1,1];
                    var year=Number(endArr[0]);
                    var month=Number((endArr[1]-1));
                    var day=Number(endArr[2]);
                  
                    var picker = new mui.DtPicker({"type": "date","beginDate":new Date(year,month,day),"endDate":new Date()}) 
                    picker.show(function(rs){
                        if(endToArr.length===1){
                            document.querySelector("#endTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                        }else{
                            var dateStart=new Date(endToArr[0],(endToArr[1]-1),endToArr[2]);
                            var dateEnd=new Date(rs.y.value,(rs.m.value-1),rs.d.value);
                            if(dateStart>dateEnd){
                                document.querySelector("#endTime").innerText='不合理时间';
                                $("#endTime").css('color','red');
                            }else{
                                document.querySelector("#endTime").innerText = rs.y.value+'年'+rs.m.value+'月'+rs.d.value+'日';
                                $("#endTime").css('color','#666');
                            }
                        }
                        picker.dispose();
                    });
                });
            })(mui);
        }

</script>

