.del_down .con{
    z-index:1116;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    min-width:320px;
    min-height: 180px;
    border-radius: 5px;
    background-color: #FFFFFF;
    opacity: 1;
    -webkit-box-shadow: 0 0 6px #ccc;
    -moz-box-shadow: 0 0 6px #ccc;
    box-shadow: 0 0 6px #ccc;
    padding: 15px 15px 30px;
}

.warn .con{width: 320px; height: 180px; }
.warn .con img{margin: 30px auto;}
.warn .con span{display: block;}


/*返回顶部样式=====================*/
a.scrollTop{
	width: 50px;
    height: 88px;
    background: url(../img/backArrow.png) no-repeat center center;
    position: fixed;
    right: 30px;
    bottom: 30px;
    z-index: 10;
    overflow: hidden;
    display: none;
}

.dh_banji {
	width: 100%;
	height: 48px;
	background: #fff;
	-webkit-box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	box-shadow: 0 0 10px #ccc;
	border-radius: 5px;
	line-height: 48px;
	font-size: 1.6rem;
	font-family: "微软雅黑";
}

.sanjiao {
	vertical-align: middle;
	margin: 0 20px;
}

.xueke_select {
	width: 100%;
	background: #fff;
	margin: 20px 0;
	font-size: 1.4rem;
	font-family: "微软雅黑";
	box-shadow: 1px 1px 5px #ccc;
	border-radius: 5px;
}

.xueke_select p:first-child{height:50px;line-height:50px;}

.xueke_select span {
	min-width: 37px;
	padding: 0 5px;
	height: 24px;
	display: inline-block;
	text-align: center;
	margin-left: 10px;
	line-height: 24px;
	cursor: pointer;
	border-radius: 2px;
	margin: 10px auto;
}
.xueke_select span:hover{background-color: #eee;}
.xueke_select span.active {
	background-color: #5093e1;
	color: #fff;
}

.xuesheng_select {
	width: 100%;
	min-height: 190px;
	background: #fff;
	-webkit-box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	box-shadow: 0 0 10px #ccc;
	border-radius: 3px;
}

.xuesheng_select .tit {
	height: 46px;
	line-height: 46px;
}

.selectks,.xueshengwrap,.dhkaishi {
	margin-left: 15px
}

.tit .biaoti {
	position: relative;
	font-size: 16px;
	font-weight: bold;
}

.tit .quanping {
	font-size: 14px;
	margin-right: 20px
}

.tit .quanping img {
	vertical-align: middle;
	margin-right: 5px;
	width: 15px;
}

.daochushuju {
	margin-left: 20px
}

.tit .biaoti:before {
	position: absolute;
	left: -15px;
	height: 24px;
	top: 10px;
	width: 3px;
	content: "";
	display: block;
	background: #5093e1;
}

.student a {
	width: 112px;
	height: 30px;
	border: 1px solid #ccc;
	display: inline-block;
	margin: 10px;
	text-align: center;
	line-height: 30px;
	border-radius: 3px;
	cursor: pointer;font-size:1.4rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.student a:hover{background-color:#eee;}
.select_foot {
	width: 100%;
	height: 37px;
	background: #f5f5f5;
	line-height: 37px;
	font-size: 14px;
}

.select_foot p {
	margin-left: 15px;
}
/*返回按钮*/
.dh_banji .back {
	margin-right: 24px;
	cursor: pointer;
}

.dh_banji .back img {
	position: relative;
	top: 12px;
}
/*4模块*/
