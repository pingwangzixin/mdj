
body{
	background: #ececec;
}
.wrapper{
	width:90%;
	min-width:1200px;
	margin:50px auto 50px;	
	box-sizing:border-box;
}
.header{
	height:50px;
	padding-left:17px;
	background: #fff;
	border-radius: 5px;
	line-height: 50px;
	font-size: 1.6rem;
	color:#000;
	-webkit-box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	box-shadow: 0 0 10px #ccc;

}
.main{
	min-height:500px;
	background: #fff;
	border-radius: 5px;
	padding-top:9px;
	padding-bottom: 20px;
	-webkit-box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	box-shadow: 0 0 10px #ccc;
	margin-top:15px;
}
.mainTitle{
	height:24px;
	line-height: 24px;
	font-size: 1.6rem;
	color:#000;
	padding-left:10px;
	border-left:3px solid #5093e1;
}
.mainTitle span{
	padding-left:10px;
	font-size: 1.4rem;
	color:#666;
}

.switingImg{
	width:100%;
	height:50px;
	margin:10px auto;
}
.switingImg img{
	width:47px;
	height:43px;
	display: block;
	margin:0 auto;
}



.tableBox{
	padding:20px 25px 0;overflow-x:auto;

}
.tableBox table{
	width:100%;
	box-sizing: border-box;
	border-collapse: collapse;
	border:1px solid #ccc;
	text-align: center;
	font-size: 1.6rem;
	color:#000;
}
.tableBox table thead th{
	padding:0;
	border:1px solid #ccc;
	height:48px;
	background: #f6f6f6;
	text-align: center;
	font-weight: normal;
}
.tableBox table tbody td{
	padding:5px;
	border:1px solid #ccc;
	text-align: center;
	height:54px;
}
.tableBox table tbody td.red{
	color:#ff3333;
}

.tableBox table tbody td i.icon-down-arrow{
	padding:10px;
	background: url(../img/icon-down-arrow.png) no-repeat center center;
}
.tableBox table tbody td i.icon-up-arrow{
	padding:10px;
	background: url(../img/icon-up-arrow.png) no-repeat center center;
}
.tableBox table tbody td i.icon-wu-arrow{
	padding:10px;
	background: url(../img/icon-wu-arrow.png) no-repeat center center;
}

.tableBox .statistics_table tr.ex-table-show{
	display: none;
}
.tableBox .statistics_table tr td.col span{
	float:left;
	display: block;
	height:50px;
	padding:0 40px;
	font-size: 1.4rem;
	color: #838383;
	line-height: 50px;
}
.tableBox .statistics_table tr td i.icon-down-open{
	width:20px;
	height:20px;
	display: inline-block;
	color: #D8D8D8;
	font-style: normal;
	cursor: pointer;
	background: url(../img/table_down_bg.png) no-repeat center center;
}
.tableBox .statistics_table tr td.expand i.icon-down-open{
	color: #4A90E2;
	background: url(../img/table_up_bg.png) no-repeat center center;
}

.tableBox table.statistics_table tbody td span.names{
	padding:5px 15px;
	float:left;
}








/*成绩列表*/
.tabBox{
	min-height:102px;
	border-radius: 5px;
	background: #fff;
	margin-top:15px;
	-webkit-box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	box-shadow: 0 0 10px #ccc;
}
.tabBox p{
	/*min-height:50px;*/
	border-bottom: 1px solid #f1f1f1;
	padding:11px 0 3px 67px;
}
.tabBox p:last-child{
	border:none;
}
.tabBox p strong{
	float:left;
	height:28px;
	line-height: 28px;
	font-size: 1.6rem;
	color:#000;
	font-weight: normal;
	margin-left:-50px;
}
.tabBox p a{
	display: block;
    float: left;
    min-width: 51px;
    padding: 0 5px;
    height: 24px;
    line-height: 24px;
    font-size: 1.6rem;
    color: #000;
    text-align: center;
    margin: 0px 3px 8px;
    border-radius: 2px;
}
.tabBox p a:hover{
	background: #eee;
}
.tabBox p a.current{
	color:#fff;
	background: #5093e1;
}

.mainTab{
	height:45px;
	background: #f5f5f5;
	border-bottom: 1px solid #ccc;
}
.mainTab ul li{
	width:110px;
	height:45px;
	float:left;
}
.mainTab ul li a{
	display: block;
	height:43px;
	line-height: 45px;
	border-top:2px solid #f5f5f5;
	border-left: 1px solid #f5f5f5;
	border-right: 1px solid #ccc;
	text-align: center;
	font-size: 1.6rem;
	color:#666666;
}
.mainTab ul li:first-child a{
	border-left: 1px solid #f5f5f5;
}
.mainTab ul li.current a{
	background: #fff;
	border-left: 1px solid #f5f5f5;
	border-right: 1px solid #ccc;
	border-top:2px solid #5093e1;
	border-bottom: 1px solid #fff;
	color:#000;
}
.mainTab ul li.current:first-child a{
	border-left: 1px solid #fff;
}
.tableBox .export{
	height:26px;
	padding:5px 0;
}
.tableBox .export a{
	display: block;
	float:right;
	padding:0 10px;
	font-size: 1.4rem;
	color:#5093e1;
}

.tableBox .gradeQuankeScollBox{
	width:100%;
	overflow-x: auto;
}

.statistics_table  tbody tr td  i.dj{
	font-size: 1.2rem;
	color:#fff;
	width:15px;
	height:15px;
	line-height: 15px;
	display: inline-block;
	font-style: normal;
	text-align: center;
	margin:0 8px;
}
.statistics_table  tbody tr td  i.dj-a{
	background: #32ba54;
}
.statistics_table  tbody tr td  i.dj-b{
	background: #2da8e3;
}
.statistics_table  tbody tr td  i.dj-c{
	background: #ffcc18;
}
.statistics_table  tbody tr td  i.dj-d{
	background: #fb5252;
}

#gradeQuanke thead th{
	height:30px;
}



