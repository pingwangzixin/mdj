/*总体分析*/
.mainTab{
	height:45px;
	background: #f5f5f5;
	border-bottom: 1px solid #ccc;
}
.mainTab ul li{
	width:220px;
	height:45px;
	float:left;
}
.mainTab ul li a{
	display: block;
	width:218px;
	height:43px;
	line-height: 45px;
	border-top:2px solid #f5f5f5;
	border-left: 1px solid #f5f5f5;
	border-right: 1px solid #ccc;
	text-align: center;
	font-size: 1.6rem;
	color:#666666;
}
.mainTab ul li:first-child a{
