/*考试管理弹窗*/
.z_del .con.z_con{padding: 22px 32px 28px 24px; width: 354px; height: 278px; font-size: 1.4rem;}
.tip{}
.tip span{font-size: 1.6rem;}
.tip i{width: 10px; height: 10px; background: url(../img/error.png) no-repeat; margin-top: 4px; cursor: pointer;}
.tip_img{margin-top: 60px;}
.tip_img img{display: block; margin: 0 auto 22px;}

.if_confirm{margin-top: 80px;}
.if_confirm span{display: inline-block; width: 132px; height: 34px; line-height: 34px; text-align: center; border: 1px solid #5395de; border-radius: 5px; overflow: hidden; cursor: pointer;}
.cancel_btn{margin-right: 48px;}
.conf_btn{color: #fff; background: #5395de;}


/*日历样式*/
.ui-widget.ui-widget-content{width: 200px;}
.ui-datepicker td span, .ui-datepicker td a {text-align:center;}
.ui-datepicker .ui-datepicker-header{background: #70abef;}
.ui-state-highlight, .ui-widget-content .ui-state-highlight{background: #fcd3a1;}
.ui-datepicker select.ui-datepicker-year{width: 40%; height: 24px; font-size: 14px; border: none; border-radius: 5px; appearance: none; -webkit-appearance: none; -moz-appearance: none; background: url(../img/se_xia.png) no-repeat 92% center #fff; padding-left: 4px; background-size: 7px 9px; margin-right: 5px; overflow: hidden;}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next{top: 4px;}
.ui-datepicker td a{font-size: 12px;}
.ui-datepicker .ui-datepicker-title{font-size: 12px;}

/*警告弹层*/
.warn .con img{
    display: block;
    /*width: 47px;*/
    margin: 40px auto 16px;
}


/*2016.11.02 新增 考查科目*/
.his{display: inline-block;display: inline-block; min-width: 74px; height: 32px; font-size: 1.4rem; line-height: 32px; text-align: center; margin-right: 10px; margin-bottom: 10px; color: #000; font-weight: bold;}
.school_year>p{font-size: 1.2rem; color: #fc8d21; line-height: 18px; padding: 0 18px;}
.add{display: block; width: 64px; height: 34px; line-height: 34px; background: url(../img/jia.png) no-repeat 7px center #ececec; border-radius: 5px; padding-left: 26px; cursor: pointer;}
.test_sub>div>p{font-size: 1.2rem; color: #fc8d21; margin: 10px 0;}
.select_box{margin-bottom: 44px; font-size: 1.4rem; position: relative;}
.no_choose{position: absolute; left: 110px; top: 8px; font-weight: normal; color: red; display: none;}
.select_group{margin-bottom: 22px;}
.select_group select{cursor: pointer; appearance: none; -webkit-appearance: none; -moz-appearance: none; font-size: 1.4rem; display: inline-block; width: 200px; height: 30px; margin-right: 16px; padding-left: 12px; border: 1px solid #ccc; outline: none; background: url(../img/se_xia.png) no-repeat 96% center;}
.select_group>select:last-of-type{width: 300px;}
.select_group select option{border-color: #ccc;}
.select_group i{display: inline-block; width: 38px; padding-left: 26px; height: 30px; line-height: 30px; background: url(../img/delete.png) no-repeat 7px center #ececec; border-radius: 5px; cursor: pointer;}


/*2016.11.07 考试管理页面 筛选学年*/
.screen_year{margin-top: 20px;}
.screen_year h3{font-size: 1.6rem; font-weight: bold; padding-left: 18px; display: inline-block;}
.screen_year>div{display: inline-block; width: 92%;}
.screen_year>div span{width: 13.5%;}


/*弹层*/
.del_down .con{
    z-index:1116;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    min-width:320px;
    min-height: 180px;
    border-radius: 5px;
    background-color: #FFFFFF;
    opacity: 1;
    -webkit-box-shadow: 0 0 6px #ccc;
    -moz-box-shadow: 0 0 6px #ccc;
    box-shadow: 0 0 6px #ccc;
    padding: 15px 15px 30px;
}



