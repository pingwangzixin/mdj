.sub_list{width: 800%; position: relative;}
.sub_list li{margin: 0 5px; text-align: center;font-size: 14px; width: 70px;}
.sub_list li img{display: block; margin: 0 auto;margin-bottom: 8px; }

.oper_btn{height: 100%; width: 12%; box-sizing: border-box; background: #DCDCDC; /*margin-top: 53px; padding-right: 20px;*/ font-size: 1.2rem; display: table; text-align: center;}
