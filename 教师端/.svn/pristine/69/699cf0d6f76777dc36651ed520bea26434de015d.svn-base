<script type="text/javascript">
    $(function () {
        addList('.wx_sureaddls1', '.weixuanzhong li', '.yixuanzhong');
        addList('.wx_sureaddls2', '.xinjianUl1 li', '.xinjianUl2');
        removeElement('.yichu1', '.yixuanzhong li');
        removeElement('.yichu2', '.xinjianUl2 li');
        clickSelect('.weixuanzhong li,.yixuanzhong li,.xinjianUl1 li,.xinjianUl2 li');
        clickInput('.weixuanzhong li input,.yixuanzhong li input,.xinjianUl1 li input,.xinjianUl2 li input');
        /*添加到组中==================*/
        function addList(btn, weixuanzhong, yixuanzhong) {
            $(btn).on('click', function () {
                var str = '';
                var html = '';
                var lis = $(weixuanzhong);
                for (var i = 0; i < lis.length; i++) {
                    if (lis.eq(i).children('input[type="checkbox"]').is(':checked')) {
                        html += '<li>' + lis.eq(i).html() + '</li>';
                    }
                }
                if ($(yixuanzhong).html()) {
                    str = $(yixuanzhong).html();
                    str += html;
                    $(yixuanzhong).html(str);
                }
                else {
                    $(yixuanzhong).html(html)
                }
                html = '';
            })
        }
        /*移除人员==================== */
        function removeElement(btn, xuanzhong) {
            $(btn).on('click', function () {
                var lis = $(xuanzhong);
                for (var i = 0; i < lis.length; i++) {
                    if (lis.eq(i).children('input[type="checkbox"]').is(':checked')) {
                        lis.eq(i).remove();
                    }
                }
            })
        }
        /*点击li的input*/
        function clickSelect(obj) {
            $(document).on('click', obj, function () {
                if ($(this).children('input').is(':checked')) {
                    $(this).children('input').prop('checked', false);
                }
                else {
                    $(this).children('input').prop('checked', true);
                }
            })
        }

        function clickInput(obj) {
            $(document).on('click', obj, function () {
                if ($(this).is(':checked')) {
                    $(this).prop('checked', false);
                }
                else {
                    $(this).prop('checked', true);
                }
            })
        }
        //显示隐藏弹框封装
        function shide(obj, state, ele) {
            $(document).on("click", obj, function () {
                if (state == "show") {
                    for (var i = 0; i < ele.length; i++) {
                        $(ele[i]).show()
                    }
                }
                if (state == "hide") {
                    for (var i = 0; i < ele.length; i++) {
                        $(ele[i]).hide()
                    }
                }
            })
        }
        shide(".wx_fabuhd", "show", ['.wx_fabuxinhd', '.wx_huise']) //发布新活动弹窗
        shide(".wx_cyglkz", "show", ['.wx_cygltcshow', '.wx_huise']) //成员管理弹窗
        shide(".wx_fabuguanbi", "hide", ['.wx_fabuxinhd', '.wx_huise']) //关闭新活动弹窗
        shide(".wx_addqz", "show", ['.wx_xinjianqunzu', '.wx_huise']) //新建群组弹窗
        shide(".wx_gbqunzu,.wx_qunzuquxiao", "hide", ['.wx_xinjianqunzu', '.wx_huise']) //关闭新建群组
        shide(".wx_deltubiao", "show", ['.wx_qrshanchule', '.wx_huise']) //确认删除弹窗
        shide(".delClose,.wx_upqxsc", "hide", ['.wx_delSureDel', '.wx_huise']) //关闭确认删除弹窗
        shide(".jiesan", "hide", ['.wx_delSureDel', '.wx_huise']) //解散  取消
        shide(".wx_cyglgb,.wx_cyglcal", "hide", ['.wx_cygltcshow', '.wx_huise']) //关闭成员管理弹窗
        shide(".wx_cyjiesan", "show", ['.wx_qrjiesanle']) //点击解散弹窗
        shide(".wx_tuichuqun", "show", ['.wx_qrtuichule', '.wx_huise']) //退出群弹窗提示
        shide(".xiaoxi", "show", ['.xiaoxibox', '.wx_huise']) //消息弹框显示
        shide(".xiaoxibox-close", "hide", ['.xiaoxibox', '.wx_huise']) //消息弹框隐藏
            //解散  取消
        $(".jiesan").on("click", function () {
                $(".wx_huise").css("z-index", 1001);
            })
            //学段和学科切换
        $(".wx_xzxd li,.wx_xzxk li,.wx_xzxktc li,.wx_xzxdtc li").on("click", function () {
                $(this).addClass("wx_xzactive").siblings().removeClass("wx_xzactive");
            })
            //群组树形结构显示和隐藏
        $(document).on("click", "span.wx_jiahao,span.wx_qzmclist ", function () {
            $(this).parent().siblings("ul").toggle();
            $(this).parent().find(".wx_jiahao").toggleClass("wx_jianhao")
        });
        //点击解散弹窗
        $(".wx_cyjiesan").on("click", function () {
                $(".wx_huise").css("z-index", 1002);
            })
            //活动选项
        $(document).on("click", ".wx_hdbiaoti", function () {
            $(".wx_hdbiaoti").map(function () {
                $(this).removeClass("active");
            });
            $(this).addClass("active");
        });
        $(document).on("change", "#dFile", function () {
            var t = "<span style='padding-left:20px'>" + $(this).val().replace(/^.+?\\([^\\]+?)(\.[^\.\\]*?)?$/gi, "$1") + "." + $(this).val().replace(/.+\./, "") + "</span>"
            $(this).parent().parent().after(t);
        });
        
        
        
        //编辑作业里的插入资源模拟下拉
		$('.search_drop ul li').each(function (){
			$(this).attr({'title' : $(this).text()});
		});
		$(document).on('click','.search_drop input',function (event){
			$('.search_drop ul').toggle();
			$('.edit_question .search_drop ul').not($(this).siblings('ul')).hide();
			event.stopPropagation();
		});
		$(document).on('click','.search_drop ul li',function (){
			$(this).parent().siblings('input').val($(this).text());
		});
		$(document).on('click',function(e) {
			$('.search_drop ul').hide();
		});
		
		
		
		
	});
</script>

</html>