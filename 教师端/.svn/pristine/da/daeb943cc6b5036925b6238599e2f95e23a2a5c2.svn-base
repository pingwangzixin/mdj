/*2016.11.28 新添班级筛选*/
.class_screen{display: inline-block; width: 16%; margin-top: 10px; position: relative;}
.class_screen span{display: inline-block; width: 95%; height: 30px; line-height: 30px; border: 1px solid #ccc; border-radius: 5px; overflow: hidden; padding-left: 10px; box-sizing: border-box; cursor: pointer; font-size: 14px;}
.class_screen span img{margin-top: 10px;margin-right: 12px;}
.class_screen span em{font-style: normal;}
.class_screen ul{width: 95%; min-width: auto; max-width: auto; height: auto; position: absolute; top: 29px;left: 0; border: 1px solid #ccc; border-radius: 5px; overflow: hidden; box-sizing: border-box; display: none;}
.class_screen ul li{float: none; height: 30px; min-height: auto; line-height: 30px; padding-left: 10px; width: 100%; background: #fff; text-align: left; padding-left: 10px; color: #000;}
.class_screen ul li:hover{background: #eee;}


