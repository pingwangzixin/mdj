/*共享至老师===============================================*/
.gongxiangzhi{
	width:668px;
	position:fixed;
	top:0;
	right:0;
	left:0;
	bottom: 0;
	margin: auto;
	max-height:380px;
	background:#fff;
	border-radius:5px;
	box-sizing:border-box;
	padding:24px 24px 74px;
	z-index:1002;
	display:none;
}
.gongxiangzhi .gongTit{height:22px;border-bottom:1px solid #eee;}
.gongxiangzhi .gongTit span{font-size:1.6rem;line-height:20px;color:#5093e1;height:22px;float:left;}
.gongClose{position:absolute;padding:10px;right:4px;top:0;cursor: pointer;}
.gongxiangzhi .gongSelectWrap{
	padding: 12px 16px 0 0;
    font-size: 1.6rem;
    margin-bottom: 10px;
}
.gongxiangzhi .gongSelectWrap span,.gongxiangzhi .gongSelectWrap select{float:left;font-size: 14px;color:#222;outline: none;height:30px;border-radius: 5px;padding-left:5px;}
.gongxiangzhi .gongSelectWrap select{
	border:1px solid #ccc;
	width:200px;
}
.gongxiangzhi .gongSelectWrap span{line-height:25px;margin-right:10px;float:left;}
.gongxiangzhi .gongAll{float:right;overflow: hidden;height:20px;line-height:20px;}
.gongxiangzhi .gongAll input{margin:7px 5px 0 0;float:left;cursor: pointer;}
.gongxiangzhi .gongAll span{float:left;font-size:1.4rem; cursor: pointer;}
.gongSeleAll{float:left;margin-top:3px;margin-right:5px;}
.gongxiangzhi .gongPersons{width:100%;box-sizing:border-box;height:202px;overflow:auto;}
.gongxiangzhi .gongPersons li{width:120px;height:20px;line-height:20px;float:left;overflow:hidden;margin-bottom:12px;}
.gongPersons span{height:20px;line-height:20px;float:left;font-size:1.4rem;line-height:1.4rem;color:#333;}
.gongPersons span label{
	display: block;
	width:70px;
	overflow: hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	cursor: pointer;
	height:20px;
	line-height:20px;
}
.gongPersons li input{margin:5px 6px 0 0;float:left;cursor: pointer;}
.gongxiangzhi .gongSub{width:100%;overflow:hidden;position:absolute;left:0;}

/*下发至学生===========================================*/
.toStudentWrap{display:none;}

/* 确认删除 提示框*/
.delSureDel .con{height:240px;width:412px;border-radius:5px;padding-top:20px;}
.delSureDel .delTishi{color:#222;font-size:16px;position:absolute;left:26px;top:20px;}
.delSureDel .delClose{position:absolute;width:13px!important;padding:12px;top:0;right:4px;margin:0!important;cursor: pointer;}
.delSureDel .tijiao{width:276px;overflow:hidden;margin: 56px auto 0;}
.delSureDel .tijiao div{float:left;width:108px;height:30px;line-height:30px;text-align:center;border-radius:5px;cursor: pointer; font-size: 14px;}
.delSureDel .tijiao .upqxsc{float:right;}
.delSureDel .tijiao .upsuresc{
	background: #fccad6!important;
	color:#e92d58;
}
.delSureDel .tijiao .upsuresc:hover{
	background: #ff98b0!important;
}

.delSureDel .con span.text-text{
	font-size: 14px;
}
