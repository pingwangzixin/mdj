body {
    color: #212121;
    position: relative;
    font-family: "microsoft yahei", Arial, sans-serif;overflow-x:hidden;
    min-width:1200px;
    overflow-x: auto;
}
.main .main_right{float:right;}
.main_right .lianjie {margin-top:12px;text-align:left;overflow:hidden;}
.main_right .lianjie a {
    float:left;
    margin-right:3%
}
.main_right .lianjie span {
    display: inline-block;
    font-size: 1.4rem;
    padding: 0 2px 2px;
    color: #949494;
    cursor: pointer;
}
.main_right .lianjie span.cur {
        color: #222222;
        border-bottom: 2px solid #5093E1;
}
.main_right .con {
    box-shadow: 0 0 10px #ccc;
    overflow:hidden;
    width: 98%;
    background-color: #fff;
    min-height: 650px;
    border-radius: 10px;
    margin-top: 10px;
    padding:0 0 50px 0;
    margin-bottom: 20px;
}

/*主题内容 右侧*/
.con {
    position: relative;
}
.con .path-navigation {
    height: 46px;
    padding-left: 10px;
    border-bottom: 1px solid #E5E5E5;
}
.con .path-navigation ul,.con .path-navigation ul li {
    float: left;
    vertical-align: middle;
    line-height: 46px;
}
.con .path-navigation ul li span {
    font-size: 14px;
    color: #212121;
}
.con .path-navigation ul li span.xie {
    margin: 0 5px;
}
.con .path-navigation ul li img {
    margin: -4px 20px 0 20px;
    vertical-align: middle;
}
.con .path-navigation .path-navigation-btn {
    float: right;
    margin-right: 20px;
}
.con .path-navigation .path-navigation-btn li {
    width: 104px;
    border: 1px solid #5EA6D9;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 10px;
    line-height: 32px;
    margin: 13px 0 0 20px;
}
.con .path-navigation .path-navigation-btn li a {
    display: block;
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    color: #5EA6D9;
}
.con .path-right {
    display: block;
    position: relative;
    float: right;
    height:40px;
    padding-top:3px;
    line-height: 40px;
    margin-right: 5%;
    overflow: hidden;
}
.con .path-right .sousuo {
	float:left;
	display: block;
    width: 20px;
    height: 20px;
    margin-top:10px;
    background:url("../img/img.png") no-repeat #F5F5F5;
    background-position:0 -117px;
    cursor: pointer;
    margin-left:-40px;
}
.con .path-right input {
    float:left;
    width: 205px;
    height: 40px;
    background: #F5F5F5;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
    border: none;
    outline: none;
    padding-left: 30px;
    padding-right:50px;
    font-size: 14px;
}
/*主题内容 右侧  table栏切换*/
.investment_f {
    margin:0 auto;
}
.investment_f .investment_title {
    height:44px;
    width:100%;
    display: inline-block;
}
.investment_title div {
    width: 25%;
    height:46px;
    float:left;
    display:inline;
    font-size:16px;
    text-align:center;
    line-height:46px;
    cursor:pointer;
    background: #F5F7F9;
    border-top: 2px solid #f9f9f9;

}
.investment_title div span {
    font-size: 14px;
}
.investment_title a,.investment_title a:link,.investment_title a:visited,.investment_title a:hover {
    color:#555;
    width:131px;
    height:44px;
    display:inline-block;
}
.investment_title div.on a,.investment_title div.on a:link,.investment_title div.on a:visited,.investment_title div.on a:hover{
    color:#f70;
}
.investment_title div.on {
    background: #fff;
    color: #4F93E0;
    border-top: 2px solid #4F93E1;
}
.investment_con {
    clear:both;
    background:#fff;
}
.investment_con_list {

}
/*主题内容 右侧 table栏切换 侧边《下载 删除》按钮*/
.investment_btn {
    width: 100%;
    min-width:936px;
    min-height: 44px;
    float: right;
}
.investment_btn dl {
    width: 39%;
    box-sizing:border-box;
    padding-left:50px;
    min-height: 21px;
    float: left;
    margin-top: 10px;
    line-height:21px;
}
.investment_btn dl dt {
    float: left;
    padding: 4px 5px 0 10px;
    font-size:14px;
    height:100%;
    margin-left:-50px;
}
.investment_btn dl dd {
    padding: 0px 10px;
    height: 21px;
    float: left;
    cursor: pointer;
    font-size:14px;
    margin:4px 2px;
}
.investment_btn dl dd.active {
    border-radius: 15px;
    background: #4F93E1;
    color: #fff;
}
.investment_btn ul {
    width: 44%;
    min-width:403px;
    min-height: 44px;
    height:100%;
    float: right;
    font-size:14px;
}
.investment_btn ul li {
    float: left;
    width:20%;
    min-width: 70px;
    box-sizing:border-box;
    text-align: center;
    /*border-left: 1px solid #ebebeb;*/
    min-height: 44px;
    height:100%;
    color: #4FC14D;
    line-height: 44px;
    cursor: pointer;
}
.investment_btn ul li i{
	color:#E5E5E5;
	float:left;
	font-style:normal ;
}
.investment_btn ul li#download,.investment_btn ul li#delete {
    min-width: 70px;
    color: #212121;
}
.investment_btn ul li#download span,.investment_btn ul li#delete span {
    display: inline-block;
    background:url("../img/img.png") no-repeat;
    height:13px;
    width:13px;
    background-position:0 -14px;
}
.investment_btn ul li#delete span {
    vertical-align: middle;
    margin-top: -2px;
    height:14px;
    width:13px;
    background-position:0 0;
}
