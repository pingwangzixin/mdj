<!--页面js-->
		<script>
			$(function(){
				//学科hover的弹出层
				function hover(opt){
					$(document).on("mouseover mouseout",opt.obj,function(event){
						if(event.type=="mouseover"){
							$(this).parent().find(opt.ele).show()
						}else if(event.type=="mouseout"){
							$(this).parent().find(opt.ele).hide()
						}
					})
				}
				hover({obj:".xuekejs",ele:".contitle"})

				//快速评价下拉选项弹出
				$(".kspj").each(function(index){
					$(this).on("mouseenter",function(){
						$(".selectpy").eq(index).show()
					})
					$(".pingyu").on("mouseleave",function(){
						$(".selectpy").eq(index).hide()
					})
				})

				//选择修改快速评价的下拉内容
				$(".selectpy li").on("click",function(){
					var pingyu= $(this).html();
					$(this).parents(".selectpy").siblings("textarea").val(pingyu)
				})
				
				
				function hoverd(obj,color,colorn){
					obj.hover(function(){
						$(this).css({"background":color})
					},function(){
						$(this).css({"background":colorn})
					})
				}
				
				hoverd($(".rwpfdel"),"#F5F5F5","#fff")
				hoverd($(".rwpfsure"),"#A5C7F1","#dfecfa")

				
				$(".pingyu textarea").each(function(index){
                    $(this).keyup(function(){
                        var num=140
                        var zishu=$(this).val().length
                        console.log(zishu)
                        if(zishu=num){}
                    })
                    
                    $(this).keydown(function(){
                    	var num=140
                        var zishu=$(this).val().length

                        if(zishu>num){
                            if(event.keyCode=="8"){
                                console.log(zishu)
                            }else{
                                alert("字数超出")
                                return false;
                            }
                        }
                    })
                })

				
				
			})
		</script>
