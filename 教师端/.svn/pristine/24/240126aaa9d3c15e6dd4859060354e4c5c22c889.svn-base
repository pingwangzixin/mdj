	</body>
	
	
	<script type="text/javascript">
		$(function(){
			$(document).on("click",".show-it",function(){
				$(".yulan").show();
			});
			$(document).on("click",".close-it",function(){
				$(".yulan").hide();
			});
			//关闭按钮
			$(document).on('click','.delClose,.wx_upqxsc',function(){
				$('.wx_delSureDel').hide();
