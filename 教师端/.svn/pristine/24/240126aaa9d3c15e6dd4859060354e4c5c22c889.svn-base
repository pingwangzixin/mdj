		<script type="text/javascript">
	        $(function(){
	        	//实例化编辑器
			    //建议使用工厂方法getEditor创建和引用编辑器实例，如果在某个闭包下引用该编辑器，直接调用UE.getEditor('editor')就能拿到相关的实例
			    var ue = UE.getEditor('editor');
			    //设置高度
			    ue.on('focus',function(){
	 				this.setHeight(400);
	 			});
	        });
	    </script>
		<style type="text/css">
			/*编辑器告诉限制样式*/
			div[id*='editor']{
		 		min-height: 400px;
		 	}
		 	div[id*='editor'] > div:first-of-type > div.edui-editor-iframeholder{
		 		max-height: 400px;
		 		overflow: hidden;
		 	}
			/*弹层样式*/
			.yulan-hide{
				position: fixed;
				top: 0px;
				left: 0px;
				right: 0px;
				bottom: 0px;
				z-index: 9999;
			    background-color: #000;
			    opacity: 0.3;
			}
			.yulan-cont{
				position: fixed;
				top: 10px;
				left: 0px;
				right: 0px;
				bottom: 10px;
				z-index: 9999;
			    background-color: #FFFFFF;
			    width: 80%;
			    min-width: 950px;
			    overflow: hidden;
			    padding: 20px;
			    margin: auto;
			}
			.yulan-cont > span{
				color: #5093e1;
				display: inline-block;
				font-size: 18px;
			}
			.yulan-cont > span.save{
				
			}
			.yulan-cont > img.close-it{
				float: right;
				cursor: pointer;
			}
			.text-html{
				border:1px solid #ddd;
				padding: 8px;
				margin-top: 20px;
				height: 90%;
				overflow: auto;
			}
			.text-html > .tit{
				text-align: center;
				margin-top: 20px;
				font-size: 20px;
			}
			/*参考教案 搜索。。。 加载。。。*/
			.search{
				width: 70%;
				height: 20px;
				border: 1px solid #DDDDDD;
				border-radius: 5px;
				margin: 10px auto 0;
				position: relative;
				background:#fff;
			}
			.search input{
				display: inline-block;
				width: 85%;
				height: 20px;
				outline: 0;
				border: 0;
				position: absolute;
				top:0;
				left:0;
				border-radius: 5px;
				
			}
			.search img{
				display: inline-block;
				width: 14px;
				height: 14px;
				position: absolute;
				top:3px;
				right:5px;
				background:#fff;
			}
			.load-move{
				position: absolute;
				bottom: -47px;
				left: 10%;
				display: block;
				height: 30px;
				border: 0;
				outline: 0;
				cursor: pointer;
				background-color: transparent;
				color: #999;
				width: 80%;
				border-radius: 2px;
			}
			/*路径提示。。。*/
			.path-navigation{
				display: block;
				height: 30px;
				line-height: 30px;
				border-bottom: 1px solid #ddd;
				margin-bottom: 10px;
			}
			.path-navigation .xie{
				margin:0 5px;
			}
			.path-navigation ul li{
				float: left;
			}
			.path-navigation ul li span{
				font-size: 14px;
				color: #212121;
			}
			
			.wx_fl{float:left}
			.wx_fr{float:right;}
			.wx_clear{clear:both;}
			.wx_ft14{font-size:14px;}
			.wx_height_25{height:25px;}
			.wx_bkyq{height: auto;padding: 20px;background: #f5f5f5;border: 1px solid #e5e5e5;border-radius: 5px;}
			.wx_btn{width: 50px;height: 25px;background: #caecca;border: none;border-radius: 10px;color: #29b529;outline:none;cursor: pointer;}
			.wx_border{border: 1px solid #e5e5e5;}
			.wx_height_50{height:50px;}
			.wx_tj_btn{width:98px;height:33px;border: none;outline:none;cursor: pointer;background:#cce0f6;color:#5093e1;border-radius:20px;}
		</style>
