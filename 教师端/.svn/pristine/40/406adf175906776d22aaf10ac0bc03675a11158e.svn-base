    <script>
    $(document).on('click','#newPlan',function(){
		$(this).find('.list').toggle();
	});
       function inputAll(config){
			$(config.inputQx).on("click",function(){
				if($(this).is(":checked")){
					$(config.inputDx).prop("checked",true);
				}else{
					$(config.inputDx).prop("checked",false);
				}
			});
			$(config.inputDx).on("click",function(){
				if($(config.inputDx).prop("checked")==""){
					$(config.inputQx).prop("checked",false);
				}
				var ls=$(config.inputDx).length;
				var chs=$(config.inputDx+":checked").length;
				if(ls==chs){
					$(config.inputQx).prop("checked",true);
				}else{
					$(config.inputQx).prop("checked",false);
				}
			});
		}
    	//我的上传全选
		inputAll({
			"inputQx":"input[name='scqx']",
			"inputDx":"input[s='sc']"
		});
		//收藏与共享
		inputAll({
			"inputQx":"input[name='gxqx']",
			"inputDx":"input[s='gx']"
		});
		//
		inputAll({
			"inputQx":"input[name='gxqxs']",
			"inputDx":"input[s='gxs']"
		});
		
		//共享至同学全选
		inputAll({
			"inputQx":"input[name='gxtxqx']",
			"inputDx":"input[gx='tx']"
		});
		
		
		
		
		
        
		 //tab栏切换函数
		    function tabs(tabTit,on,tabCon) {
		        $(tabCon).each(function () {
		            $(this).children().eq(0).show();
		        });
		        $(tabTit).each(function () {
		            $(this).children().eq(0).addClass(on);
		        });
		        $(tabTit).children('div').click(function () {
		            $(this).addClass(on).siblings().removeClass(on);
		            var index = $(tabTit).children().index(this);
		            $(tabCon).children().eq(index).show().siblings().hide();
		        });
		    }
		    tabs(".investment_title","on",".investment_con");
		   </script>
