/*字体大小*/
.font14{font-size: 1.4rem;}
p{word-wrap:break-word; padding:0 1px}
/*------------------------------------ 作业列表  -------------------------------------------*/

html{font:normal normal 62.5% "Microsoft YaHei";color:#222222;}/*加上是--1rem等于10px*/
.hand{cursor: pointer;}
.main_right .list_tit{text-align: left;margin-top: 12px;}
.main_right .list_tit li{width: 9%;float: left;}
.main_right .list_tit li a{
	display: inline-block;
	font-size: 1.4rem;
	padding: 2px;
	color:#949494;
	cursor: pointer;
}
.main_right .list_tit li a.list_tit_active,.job .but a.list_tit_active{
	border-bottom: 2px solid #5093e1;
	color:#222;
}

.task_list{
	position: relative;
	width: 94%;
	background-color: #fff;
	min-height: 600px;
	border-radius: 10px;
	margin-top: 10px;
	padding:16px 10px;
	margin-bottom: 20px;
	box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-webkit-box-shadow: 0 0 10px #ccc;
	-o-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
}

.task_list_tit>p{font-size: 1.4rem;}
.task_list_tit>p em{font-style: normal;}

.task_list .del_statistics{}
.task_list .search_create{width: 100%;margin-top: -9px;}
.task_list .del_statistics li{margin-left: 32px;cursor: pointer;}
.task_list .del_statistics li span{font-size: 1.4rem;line-height: 2px;}


.task_list .search{
	display: block;
	width: 32%;
	height: 38px;
	border-radius: 18px;
	background-color: #f5f5f5;
	position: relative;
}
.task_list .search input{
	display: inline-block;
	width: calc(100% - 55px);
	height: 38px;
	border-radius: 18px;
	background-color: #f5f5f5;
	border:0;
	text-indent: 15px;
	outline: none;
	font-size: 1.4rem;
}
.task_list .search img{
	display: inline-block;
	position: absolute;
    top: 10px;
    right: 20px;
}
.task_list .create{
	width: 10%;
    height: 29px;
    line-height: 28px;
    text-align: center;
    background-color: #4f94e0;
    color: #fff;
    border-radius: 6px;
    font-size: 1.4rem;
    margin-top: 6px;
    cursor: pointer;
}
.task_list li.que{
	width: 15%;
    height: 29px;
    line-height: 28px;
    text-align: center;
    background-color: #4f94e0;
    color: #fff;
    border-radius: 6px;
    font-size: 1.4rem;
    margin-top: 6px;
    cursor: pointer;
    margin-right: 4px;
}
/*.task_list li.que:last-child{margin-right: 10px;}*/
.task_list li.que:hover{background-color:#0050ac;}
.task_list li.que a{
	display: block;
	color:#fff;
	width: 100%;
    height: 29px;
    line-height: 28px;
}
.task_list .create:hover,.task_list .create:active{
	background-color:#0050ac;
}
/*表格*/
.task_list .list_table{width:100%;text-align: center;margin-top: 10px;}
.task_list .list_table input{position: relative;top: 3px;left: -4px;cursor: pointer;}
.task_list .list_table label{cursor: pointer;}
.task_list .list_table thead th{height:44px;border:1px solid #cccccc;text-align: center;font-size: 1.4rem;background-color: #f5f5f5;}
.task_list .list_table thead th:nth-child(1){width: 6%;}
.task_list .list_table thead th:nth-child(2){width: 12%;}
.task_list .list_table thead th:nth-child(3){width: 10%;}
.task_list .list_table thead th:nth-child(4){width: 9%;}
.task_list .list_table thead th:nth-child(5){width: 6%;}
.task_list .list_table thead th:nth-child(6){width: 13%;}
.task_list .list_table thead th:nth-child(7){width: 10%;}
.task_list .list_table thead th:nth-child(8){width: 16%;}

.task_list .list_table tbody td{
	height:44px;
	border:1px solid #cccccc;
	text-align: center;
	font-size: 1.4rem;
}
/*.task_list .list_table tbody td.hand a:hover{color:#5093E1;}*/
.task_list .list_table tbody td.hand a{color:#5093E1;}
.task_list .list_table tbody td img{cursor: pointer;}
.task_list .list_table tbody td a{display: inline-block;width: 100%;height: 100%;line-height: 44px;}

/*1行的样式*/
.task_list .list_table tbody .Line1  td{height: 180px;}
.task_list .list_table tbody .Line1  td a{line-height: 180px;}
/*2行的样式*/
.task_list .list_table tbody .Line2  td{height: 90px;}
.task_list .list_table tbody .Line2  td a{line-height: 90px;}
/*3行的样式*/
.task_list .list_table tbody .Line3  td{height: 60px;}
.task_list .list_table tbody .Line3  td a{line-height: 60px;}
/*4行的样式*/
.task_list .list_table tbody .Line4  td{height: 45px;}
.task_list .list_table tbody .Line4  td a{line-height: 45px;}

/*搜索无内容*/
.wu_neirong{
	text-align: center;
	font-size: 1.4rem;
	color:#555;
}
.wu_neirong .neirong{text-align: left;width: 35%;margin: 100px auto;}
.wu_neirong strong{font-size: 1.8rem;color:#000;}
.wu_neirong strong span{color:#f65177;}
.wu_neirong  p{margin: 20px 0 10px 0;}
.wu_neirong li{line-height: 24px;}


/* 创建试题*/
.create_box{padding:16px 20px;position: relative;}
.create_box .create_tit{width:100%;border-bottom:1px solid #e5e5e5;}
.create_box .create_tit span.creat{
	display: inline-block;
	color:#5093e1;
	font-size: 1.6rem;
	line-height: 30px;
	border-bottom:2px solid #e5e5e5;
}
.create_box .libie li{
	width: 100%;
	line-height: 47px;
	font-size: 1.4rem;
	color:#333;
}
.create_box .libie li input{
	display: inline-block;
	outline: none;
	border:1px solid #e5e5e5;
	width: 328px;
	height: 28px;
	text-indent: 10px;
	border-radius: 6px;
	font-size: 1.4rem;
	color:#9c9c9c;
	margin-left: 5px;
}
.create_box .libie li select{
	/*width: 12%;
	min-width: 94px;*/
	width: 132px;
	border:0 none;
	outline: none;
	height: 30px;
	padding-left: 10px;
	font-size: 1.4rem;
	color:#333;
	border-radius: 5px;
	border:1px solid #e5e5e5;
	margin-right: 22px;
}
/*编辑器--单选题，多选题*/
.create_box .stem{border:1px solid #d0d0d0;}
.create_box .stem h3{
	width: 98.1%;
	height: 44px;
	line-height: 44px;
	border-bottom:1px solid #d0d0d0;
	font-size: 1.4rem;
	padding-left: 20px;
}
.create_box .stem .radios{width: 48%; margin-right: 4%;}
.create_box .stem .radios:nth-child(even){margin-right: 0;}
.create_box .stem .radios h4{
	width: 95.7%;
	height: 44px;
	line-height: 44px;
	font-size: 1.4rem;
	padding-left: 20px;
}
.create_box .stem input{ position: relative;top: 2px;}
.create_box  .stem1{margin-top: 26px;}
.create_box .stem1 h3 span{
	display: inline-block;
	width: 100px;
	height: 34px;
	line-height: 34px;
	text-align: center;
	background-color: #cce0f6;
	color:#5093e1;
	font-size: 1.4rem;
	border-radius: 15px;
	margin-top: 5px;
	border:0;
}
.create_box .stem1 h3 span:first-of-type{margin:5px 4px 0 14px;}

/*判断题*/
.create_box .daan{
	font-size: 1.4rem;
	color:#333;
	margin-top: 26px;
}
.create_box .daan span{margin-right: 20px;}
.create_box .daan input{position: relative;top:2px;}
/* 填空题 */
.blanks .stem h3 span#addtkx{
	margin:0 10px;
	font-size: 14px;
	color:#fff;
	background: #0066CC;
	padding:3px 10px;
	border-radius: 5px;
	cursor: pointer;
}
.blanks .stem h3 span#hqxxs{
	margin:0 10px;
	font-size: 14px;
	color:#fff;
	background: #0066CC;
	padding:3px 10px;
	border-radius: 5px;
	cursor: pointer;
}
/* 插入分割线 */
.blanks .stem h3 span#addfgx{
	height:22px;
	line-height: 22px;
	margin:0 10px;
	font-size: 14px;
	color:#fff;
	background: #0066CC;
	padding:3px 10px;
	border-radius: 5px;
	cursor: pointer;
}



#editor8 input[tkt="tk"]{
	width:200px;
	border:none;
	border-bottom:1px solid #333;
	display:inline-block;
	text-align: center;
	font-size: 14px;
	color:#333;
}
/*完形填空题*/
.create_box .options{padding: 20px 0;border-top: 1px solid #e5e5e5;}
.create_box .options:first-of-type{border-top:0;}
.create_box .options .tit{
	width: 8%;
    text-align: center;
}
.create_box .options ul.cons,.create_box .options ul.cont{width: 60%;}
.create_box .options ul.cons li:last-of-type,.create_box .options ul.cont li:last-of-type{margin-top: 22px;}
.create_box .options ul.cons li span,.create_box .options ul.cont li span{display: inline-block;width: 45%;}
.create_box .options ul.cons li span input[type=text],.create_box .options ul.cont li span input[type=text]{
	width: 80%;
	height: 27px;
	outline: none;
	border: 1px solid #e5e5e5;
	border-radius: 5px;
	text-indent: 10px;
}
/*材料题*/
.create_box .options textarea.tex{
	width: 90%;
	height: 50px;
	display: block;
	outline: none;
	border:1px solid #e5e5e5;
	background: #fff;
	padding: 5px;
}
/*阅读理解*/
.create_box .read .options ul{width: 94%;margin: auto;}
.create_box .read .options ul.cons li input[type=text],.create_box .read .options ul.cont li input[type=text]{
	width: 94%;
	height: 27px;
	outline: none;
	border: 1px solid #e5e5e5;
	border-radius: 5px;
	text-indent: 10px;
	margin-top: 15px;
}
.create_box .read .options ul.cons li:last-of-type,.create_box .read .options ul.cont li:last-of-type{margin-top: 0px;}

.create_box .read .read_tit{padding-left: 10px;padding-top: 10px;width: 20px;}

/*创建试题--按钮*/
.create_box .create_btn{
	text-align: center;
    width: 300px;
    margin-top: -8px;
}
.create_box .create_btn span{
	display: inline-block;
	width: 100px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	border-radius: 6px;
	font-size: 1.4rem;
	cursor: pointer;
}
.create_box .create_btn span:first-of-type{
	background-color:#dfecfa;
	color:#5093e1;
}
.create_box .create_btn span:last-of-type{
	border:1px solid #e5e5e5;
	color:#333;
	margin-left: 30px;
}
.create_box .create_btn span:first-of-type:hover{
	background-color:#A5C7F0;
}
.create_box .create_btn span:last-of-type:hover{
	background-color:#F5F5F5;
}


/* 编辑任务   */
.job{
	padding: 18px  26px 10px 40px;
	height:24px;
}
.job .but{width: 50%;}
.job .but a{
	display: inline-block;
	color: #949494;
	font-size: 1.4rem;
	margin-right: 30px;
}
.job .but a.but_active{
	color: #222;
	border-bottom: 2px solid #5093e1;
	line-height: 22px;
}

.job .back{cursor: pointer;position: relative;top: 3px;margin-top: 3px;}
.job .back span{color:#5093e1;font-size: 1.4rem;}

.job a .back{
	position: relative;
    top: 3px;
    margin-right: 15px;
}
.edit_task{
	width: 97%;
	background-color: #fff;
	min-height: 780px;
	border-radius: 10px;
	margin: 0px auto;
	box-shadow: 0 0 10px #ccc;
	-moz-box-shadow: 0 0 10px #ccc;
	-webkit-box-shadow: 0 0 10px #ccc;
	-o-box-shadow: 0 0 10px #ccc;
	-ms-box-shadow: 0 0 10px #ccc;
	margin-bottom: 30px;
    padding-bottom: 15px;
}
.edit_task .edit_tit{border-bottom: 1px solid #e5e5e5;padding:16px 20px 4px 20px;}
.edit_task .edit_tit p{
	font-size: 1.4rem;
}
.edit_task .edit_tit ul li{
	float: left;
	font-size: 1.4rem;
	line-height: 40px;
	margin-right: 30px;
}
.edit_task .edit_tit ul li:last-child input{box-sizing: content-box; outline: none; border: 1px solid #e3e3e3; overflow: hidden; display: inline-block; width: 24px; height: 20px; line-height: 20px; padding: 0 10px; background: #e3e3e3; border-radius: 10px; text-align: center;}
/*.edit_task .edit_tit ul li em{font-style: normal; vertical-align: middle; }*/
/*.edit_task .edit_tit ul li:first-of-type{margin-right: 30px;}*/
.edit_task .edit_tit ul li img{margin-top: 8px;}
.edit_tit .edit_btn span{
	display: inline-block;
	width: 60px;
	height: 25px;
	line-height: 24px;
	text-align: center;
	border-radius: 20px;
	font-size: 1.4rem;
	cursor: pointer;
	margin-top: 14px;
}
/*各个颜色按钮*/
.edit_tit .edit_btn span.blue{
	background-color: #cadef6;
	color:#5093e1;
}
.edit_tit .edit_btn span.green{
	background-color: #caecca;
	color:#50c14e;
	margin-right:5px;
}
.edit_tit .edit_btn span.pink{
	background-color: #fccad6;
	color:#e92d58;.
}
/*各个颜色按钮*/

.edit_task .edit_box{
	width: 80%;
    margin: 36px 10% 0;
}
