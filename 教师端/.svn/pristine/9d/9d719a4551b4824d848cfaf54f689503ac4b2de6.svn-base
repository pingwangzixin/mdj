.YsaveBox .con{width: 400px; height: 490px; padding: 45px 25px 0; box-sizing: border-box; border-radius: 5px; background: #fff; overflow: hidden; position: absolute; top: 50%; left: 50%; margin-left: -200px; margin-top: -245px;}
.YsaveBox b,.YsaveBox em{font-style: normal;}
.YsaveBox strong{font-weight: normal;}
.YsaveBox .con>div>em{font-size: 16px;}
.Ytextclose{position: absolute; top: 14px; right: 16px; cursor: pointer;} 
.Ypaper_title{margin-bottom: 16px;}
.Yspot{display: inline-block; width: 6px; height: 6px; border-radius: 50%; overflow: hidden;background: #000; margin-bottom: 2px;}
.Yedit_tit{display: inline-block; border-bottom: 1px solid #e5e5e5; width: 246px;}
.Yedit_tit input{width: 90%; box-sizing: border-box; border: none; outline: none; padding-left: 5px; font-size: 14px;}
.Yedit_tit img{cursor: pointer;}
.Yscore_setting{}
.Yscore_setting ul{margin-top: 10px; padding-left: 10px; font-size: 14px;}
.Yscore_setting ul li{line-height: 24px; margin-bottom: 16px;}
.Yscore_setting ul li input{width: 60px; border: none; border-bottom: 1px solid #e5e5e5; text-align: center; outline: none;}
.Yscore_setting ul li i{font-style: normal; display: none; color: #f65177; margin-left: 10px;}
.Ytotal_score{text-align: right;}
.Ytotal_score span{font-size: 30px; color: #5093e1;}
.Ytips{color: #f65177; margin-top: 30px; line-height: 22px; border-bottom: 1px solid #e5e5e5;}

.Ytextbtn{text-align: center; margin-top: 30px; font-size: 1.4rem;}
.Ytextbtn span{display: inline-block; width: 110px; height: 30px; line-height: 30px; border-radius: 5px; overflow: hidden; text-align: center; cursor: pointer;}
.Ytextbtn .Ysure{background: #dfecfa; color: #5093e1; margin-right: 50px;}
.Ytextbtn .Ycancel{border: 1px solid #e5e5e5;}
.Ytextbtn .Ysure:hover{background: #a5c7f1;}
.Ytextbtn .Ycancel:hover{background: #f5f5f5;}

.Yuse_tutorial_box{}
.Yuse_det{width: 1200px; position: fixed; top: 0; left: 0; right: 0; bottom: 0; margin: auto;}
.Yuse_det img{position: absolute;}
.Yuse_close{top: 66px; right: 52px; cursor: pointer;}
.Yuse_prev{display: none; top: 546px; left: 168px; cursor: pointer; z-index: 1;}
.Yuse_next{right: 0; top: 800px; right: 70px; top: 560px; cursor: pointer;}
.Yuse_main_pic{top: 0; left: 0; right: 0; bottom: 0; margin: auto;}
