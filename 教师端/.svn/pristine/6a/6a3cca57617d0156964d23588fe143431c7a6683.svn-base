/*社团详情页面css*/
html,body{font-size: 1.4rem;}
.shetuan {
    width: 97%;
    margin: 20px auto;
    min-width: 1200px;
}
.shetuanul li {
    display: inline-block;
    color: #949494;
    margin-right: 30px;
    font-size: 14px;
}
.shetuanul li.active {
    color: #000;
    border-bottom: 2px solid #5093e1;
}
.stxiangqing {
    margin-top: 10px;
    width: 100%;
}
.myshetuan {
    min-width: 15%;
    max-width: 15%;
    min-height: 500px;
    background: #fff;
    border-radius: 10px;
    float: left;
    overflow-y: auto;
    overflow-x: hidden;
}
.sttreeheader {
    height: 35px;
    background: #5093e1;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    line-height: 35px;
    text-align: center;
    color: #fff;
    font-size: 16px;
}
.addst {
    height: 40px;
    background: #e5e5e5;
    line-height: 40px;
    text-align: center;
    color: #555;
    font-size: 14px;
    cursor: pointer;
}
.stlist ul li {
    height: 40px;
    border-bottom: 1px solid #e5e5e5;
    line-height: 40px;
    text-align: center;
    cursor: pointer;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.stlist ul li:hover {
    color: #5093e1!important;
}
.stlist ul li.xuanzhong {
    color: #5093e1!important;
}
.mystjs {
    width: 82%;
    background: #fff;
    min-height: 500px;
    float: left;
    margin-left: 25px;
    border-radius: 2%;
}
.stjianjie,
.jiaoshijj {
    padding: 20px;
}
.jianjieheader {
    height: 40px;
    line-height: 40px;
}
.fleft {
    float: left;
}
.fright {
    float: right;
}
.lan {
    color: #5093e1;
}
.ft-sz16 {
    font-size: 16px;
}
.ft-sz15 {
    font-size: 15px;
}
.shetuanbianji {
    min-height: 200px;
    background: #fff;
    text-indent: 30px;
    line-height: 30px;
    word-wrap: break-word;
    white-space: normal;
}
.shetuanbianji div {
    text-indent: 30px;
    line-height: 30px;
    word-wrap: break-word;
    white-space: normal;
}
.hrxian {
    background: #ddd;
    width: 100%;
    height: 1px;
}
.jiaoshitouxiang {
    float: left;
    margin-right: 10px;
}
.jiaoshibianji {
    min-height: 200px;
    background: #fff;
    text-indent: 30px;
    line-height: 30px;
}
.jiaoshibianji div {
    text-indent: 30px;
    line-height: 30px;
    word-wrap: break-word;
    white-space: normal;
}
.jiaoshibianji p,
.shetuanbianji p {
    text-indent: 30px;
    line-height: 30px;
}
.bianjijiaoshi {
    min-height: 200px;
}
span {
    display: inline-block;
}
.bianji {
    width: 16px;
    height: 16px;
    background: url(../img/bianji.png)no-repeat;
    margin-right: 5px;
}
.baocun {
    width: 16px;
    height: 16px;
    background: url(../img/baocun.png)no-repeat;
    margin-right: 5px;
}
.pointer {
    cursor: pointer;
}

/*管理成员页面css*/

.shetuanchengyuan {
    width: 80%;
    background: #fff;
    min-height: 500px;
    float: left;
    margin-left: 25px;
    border-radius: 2%;
}
.chengyuanlist ul li {
    height: 85px;
    border: 1px solid #e5e5e5;
    width: 22%;
    display: inline-block;
    margin: 10px;
    position: relative;
}
.fleft {
    float: left;
}
.cytouxiang {
    margin-left: 10px;
    margin-top: 15px;
    margin-right: 10px;
}
.cyname {
    margin-top: 15px;
    line-height: 25px;
}
.shuxian {
    margin: 0 10px;
}
.delchengyuan {
    position: absolute;
    bottom: 0;
    right: 0;
    cursor: pointer;
}
.xingbie {
    margin-left: 20px;
}
.addchengyuan {
    height: 85px;
    border: 1px solid #e5e5e5;
    width: 22%;
    display: inline-block;
    margin: 10px;
    position: relative;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
}
.addtubiao {
    margin-top: 15px;
}
.file {
    position: relative;
    display: inline-block;
    padding: 4px 12px;
    overflow: hidden;
    color: #1E88C7;
    text-decoration: none;
    text-indent: 0;
    line-height: 20px;
}
.file input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
}
#jiaoshitouxiang,#jiaoshitouxiang2 {
    width: 127px;
    height:127px;
    border: 1px solid #ebebeb;
}
.st-nan{
    width: 50px;
    height: 50px;
    background: #ABCBF1;
    color: #629de1;
    font-size: 20px;
    line-height: 50px;
    text-align: center;
    border-radius: 5px;
}
.st-nv{
    width: 50px;
    height: 50px;
    background: #FBACBE;
    color: #F65177;
    font-size: 20px;
    line-height: 50px;
    text-align: center;
    border-radius: 5px;
}

.jiesanshetuan{
    display: inline-block;width: 100px;height: 30px;background: #5093e1;line-height: 30px;text-align: center;color: #fff;border-radius: 18px;float: right;margin-bottom: 10px;margin-top: 10px;margin-right: 20px;cursor:pointer;
}