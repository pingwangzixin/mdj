html{font:normal normal 62.5% "Microsoft YaHei";color:#222222;}/*加上是--1rem等于10px*/
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,input,button,textarea,p,th,td{padding:0;margin:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset,img{border:0}
a{text-decoration:none;outline:none;color:#222;}
a,input{-webkit-tap-highlight-color:rgba(2,2,2,0);}/*清除a标签点击之后的阴影*/
img{ border:none; vertical-align:top;max-width: 100%;}
textarea{ resize:none; overflow:auto;}
li{list-style:none;}
caption,th{text-align:left;}
h1,h2,h3,h4,h5,h6{font-weight:normal;}
input,button,textarea,select,optgroup,option{font-family:inherit;font-size:inherit;font-style:inherit;font-weight:inherit;}
input,button,textarea,select{*font-size:100%;}
table{ border-collapse:collapse;}
.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
  clear: both;
}
.clearfix{*zoom:1;}
.fl{float: left;}
.fr{float: right;}


/*版本的判断*/
.con.shade{width:410px; height:252px; color: #222; font-size:16px;}
.sahdeHead{padding:17px 17px 40px 25px;}
.sahdeHead span{float: left; font-size:16px;}
.sahdeHead img{float: right;}
.shadeWarn p{line-height: 24px;}
.shadeWarn img{padding-bottom:14px;}
.shadeWarn p a{color:#f65177;text-decoration:underline ;}
.topNote{background: #838383; height: 43px; width: 100%; color: white; padding-left: 40px;line-height: 43px; font-size: 16px; position: fixed; top: 0;}
.topNote span a{color: white; text-decoration: underline;}
.topNote span{float: left;}
.topNote p{position: absolute; right: 56px; top:10px;}
.topNote p img:nth-of-type(2){position: absolute; right: 5px;top: 7px;}





