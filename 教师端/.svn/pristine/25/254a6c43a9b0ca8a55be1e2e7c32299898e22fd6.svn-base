	<script>
		//创建作业二级弹框 复选框
		$(document).on('click','.task_two .con_con .libie li em',function (){
			if($(this).hasClass('active')){
				$(this).removeClass('active').parent().nextAll().find('em').removeClass('active');
				$(this).siblings('input').attr({'checked':false}).parent().nextAll().find('input').attr({'checked':false});
			}else{
				$(this).addClass('active').parent().prevAll().find('em').addClass('active');
				$(this).siblings('input').attr({'checked':true}).parent().prevAll().find('input').attr({'checked':true});
			}
		});
		
