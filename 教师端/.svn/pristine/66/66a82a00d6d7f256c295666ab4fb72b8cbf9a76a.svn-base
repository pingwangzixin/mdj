body {
    color: #212121;
    position: relative;
    font-family: "微软雅黑", Arial, sans-serif;
    font-size: 14px;
}
.main .main_right{float:right;}
.main_right .lianjie {margin-top:12px;text-align:left;overflow:hidden;}
.main_right .lianjie a {
   float:left;
}
.main_right .lianjie span {
    display: inline-block;
    font-size: 1.4rem;
    padding: 0 2px 2px;
    color: #949494;
    cursor: pointer;
}
.main_right .lianjie span.cur {
        color: #222222;
        border-bottom: 2px solid #5093E1;
}


/*主体内容*/

.container {
    
    /*box-shadow: 0 0 10px #ccc;*/
   /* overflow: hidden;*/
    width: 98%;
    border-radius: 10px;
    margin-top: 10px;
    /*background-color: #fff;*/
}

.container .con-left {
    width: 61%;
    background: #fff;
    min-height: 635px;
    -webkit-box-shadow: 0 0 10px #ccc;
    -moz-box-shadow: 0 0 10px #ccc;
    box-shadow: 0 0 10px #ccc;
    float: left;
    padding: 10px 20px 50px 20px;
    border-radius: 10px;
    position: relative;
}

.container .con-right {
    width: 34%;
    background: #fff;
    min-height: 670px;
    -webkit-box-shadow: 0 0 10px #ccc;
    -moz-box-shadow: 0 0 10px #ccc;
    box-shadow: 0 0 10px #ccc;
    float: right;
    border-radius: 10px;;
}


/*时间 list切换*/

.con-left .conleft-list {
    overflow: hidden;
    margin-bottom: 8px;
}

.con-left .conleft-list span {
    margin-top: 5px;
    float: left;
}

.con-left .conleft-list ul {
    float: left;
    margin-top: 5px;
}

.con-left .conleft-list ul li {
    float: left;
    font-size: 14px;
    color: #212121;
}

.con-left .conleft-list ul li {
    float: left;
    width: 52px;
    height: 22px;
    margin-left: 10px;
}

.con-left .conleft-list ul li a {
    width: 52px;
    line-height: 22px;
    display: block;
    font-size: 14px;
    color: #212121;
    text-align: center;
}

.con-left .conleft-list ul li.current a {
    background: #4F93E1;
    color: #fff;
    border-radius: 11px;
}

.con-left .conleft-list form div {
    position: relative;
    float: right;
}

.con-left .conleft-list input[type="text"] {
    width: 222px;
    height: 36px;
    border: 1px solid #ccc;
    border-radius: 17px;
    outline: none;
    padding-left: 10px;
}

.con-left .conleft-list span.form-tijiao {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 5px;
    right: 15px;
    background: url(../img/img.png) no-repeat 0 -117px;
    cursor: pointer;
}


/*list 切换内容*/

.conleft-nav>ul>li {
    width: 100%;
    border-left: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    border-right: 1px solid #ccc;
    overflow: hidden;
}

.conleft-nav>ul>li:nth-child(1) {
    border-top: 1px solid #ccc;
}

.conleft-nav ul li div {
    float: left;
    
}
.conleft-nav ul li div.connav-left {
	float: left;
   box-sizing:border-box;
   padding: 15px 0px 15px 10px;
}

.conleft-nav ul li div.connav-right {
    float: right;
    box-sizing:border-box;
    padding: 15px 10px 15px 0px;
}

.connav-right p {
    display: inline-block;
    float: left;
    margin-right: 10px;
}

.connav-right ul {
    float: left;
    /*width: 74%;*/
    min-width:240px;
}

.connav-right ul li {
    overflow: hidden;
    white-space: nowrap;
    width: 230px;
    height: 18px;
    text-overflow: ellipsis;
    margin-bottom: 10px;
}

.connav-right ul li a:hover {
    color: #5093e1;
}
.connav-right ul li a.current{
	color: #5093e1;
}
.connav-left>p {
    font-weight: bold;
    width: 250px;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 20px;
    overflow: hidden;
}

.connav-left>p span a {
    font-weight: normal;
}

.connav-left>p span a:visited {
    color: #41C7D7;
}

.connav-left-nav {
    /*    width: 300px;*/
    overflow: hidden;
    margin-top: 10px;
}

.connav-left-nav>p {
    float: left;
}

.connav-left-nav>ul {
    float: left;
}

.connav-left-nav>ul li {
    margin-bottom: 10px;
}

.connav-left-nav>ul li a {
    width: 83px;
    display: inline-block;
    text-align: center;
    line-height: 27px;
    background: #FFf;
    border: 1px solid #81D36F;
    color: #222;
    border-radius: 3px;
    margin-right: 5px;
}
.connav-left-nav>ul li a:hover{
	background: #81D36F;
	color:#fff;
}

.connav-left-nav>ul li a.current {
    color: #FFF;
    background: #81d36f;
}


/*主题内容右边*/

.con-right h3 {
    /*    width: 362px;*/
    color: #212121;
    font-weight: bold;
    line-height: 50px;
    text-align: left;
    font-size: 16px;
    padding-left: 25px;
}

.con-right .describe {
    /*    width: 347px;*/
    height: 55px;
    background: #F9F9F9;
    padding-left: 25px;
    line-height: 27px;
    color: #201D16;
}

.con-right .describe span {
    width: 88px;
    height: 27px;
    display: inline-block;
    text-align: center;
    /*background: url("../img/Personal-resources_yixuexi.png") no-repeat center left;*/
}

.con-right .describe span.weixuexi {
    /*background: url("../img/Personal-resources_weixuexi.png") no-repeat center left;*/
}


/*人物简介*/

.conright-nav {
    padding: 20px 0 0 20px;
}

.conright-nav ul li {
	width:33.3%;
    margin-bottom: 20px;
    float: left;
    box-sizing:border-box;
}

.conright-nav ul li div.touxiang{
    float: left;
    width:38px;
    height:38px;
    margin-right: 5px;
}
.conright-nav ul li div.touxiang span{
	display: block;
    width:38px;
    height:38px;
    text-align: center;
    line-height: 38px;
    border-radius: 5px;
    background: #4F93E1;
    font-size: 22px;
    color:#fff;
}
.conright-nav ul li div.touxiang span.lanse{
	background: #4F93E1;
}
.conright-nav ul li div.touxiang span.lvse{
	background: #50c14e;
}

.conright-nav ul li .introduction {
    float: left;
    width: 55%;
}
.conright-nav ul li .introduction p{
	height:20px;
	overflow: hidden;
	text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.conright-nav ul li .introduction img {
    float: right;
    margin-top: 3px;
}


.yixuexi,
.weixuexi,
.zhengzaixuexi {
    position: relative;
}

.yixuexi:before {
    display: inline-block;
    content: "";
    width: 10px;
    height: 10px;
    position: absolute;
    background: #487190;
    left: 0px;
    top: 9px;
}

.weixuexi:before {
    display: inline-block;
    content: "";
    width: 10px;
    height: 10px;
    position: absolute;
    background: #50c14e;
    left: 0px;
    top: 9px;
}

.zhengzaixuexi:before {
    display: inline-block;
    content: "";
    width: 10px;
    height: 10px;
    position: absolute;
    background: #f65177;
    left: 0px;
    top: 9px;
}
.lanse{
	color: #487190;
}
.lvse {
    color: #50c14e;
}

.hongse {
    color: #f65177;
}

.introduction,
.connav-right {
    font-size: 14px;
}