	/*ziyuan   */
ul.ziyuan,.ziyuan dl,.ziyuan dt,.ziyuan dd{
  list-style: none;
  margin: 0;
  padding: 0;
  text-align: center;
}
ul.ziyuan a {
  color: #d03d31;
  text-decoration: none;
}
ul.ziyuan a:link {
  text-decoration: none;
}
ul.ziyuan a:visited {
  text-decoration: none;
}
ul.ziyuan a:hover {
  text-decoration: none;
}
ul.ziyuan a:active {
  text-decoration: none;
}
ul.ziyuan li {
  height: 41px;
  /*border: 1px solid #EFEFEF;*/
  border-top: 0px;
  position: relative;
}
ul.ziyuan li:before,
ul.ziyuan li:after {
  content: " ";
  display: table;
  clear: both;
}
ul.ziyuan li:hover {
  background-color: #b8d9f3;
}
ul.ziyuan li:hover .ziyuan-item {
  left: 100%;
  top: -1px;
}

ul.ziyuan dt {
  height: 27px;
  font-size: 16px;
}
ul.ziyuan dt span {
    cursor: pointer;
  padding-top: 10px;
  display: block;
  color: #666;
}
ul.ziyuan dd span {
  color: #d03d31;
  display: block;
  width: 62px;
  font-size: 14px;
  margin-bottom: 3px;
  float: left;
}
ul.ziyuan .ziyuan-item {
  position: absolute;
  left: -999em;
  width: 240%;
}
ul.ziyuan .ziyuan-item > span {
    display: block;
    position: absolute;
    left: -2px;
    top: 20px;
    width: 0;
    height: 0;
    border-top: 10px solid transparent;
    border-bottom: 10px solid transparent;
    border-right: 16px solid #eaf4fc;
}
ul.ziyuan .ziyuan-item .ziyuan-dl-wrap {
  margin-left: 9px;
  background-color: #eaf4fc;
  border: 1px #EFEFEF solid;
  padding: 12px 0;
  -webkit-box-shadow: 0 0 7px #aaa;
  -moz-box-shadow: 0 0 7px #aaa;
  -ms-box-shadow: 0 0 7px #aaa;
  box-shadow: 0 0 7px #aaa;
}
ul.ziyuan .ziyuan-item .ziyuan-dl-wrap dl{padding-top:10px;padding-right:16px;box-sizing:border-box;}
ul.ziyuan .ziyuan-item dl span {
  color: #555555;
  font-size: 12px;
  padding: 0px;
  border-left: 2px solid #eee;
}
ul.ziyuan .ziyuan-item dt {
  width: 50%;
  padding-top: 0px;
  float: left;
}
ul.ziyuan .ziyuan-item dt span {
    font-size:14px;color:#555;font-weight:bold;border:0 none;
}
ul.ziyuan .ziyuan-item dt span.cur {
    color: #d03d31;
}
ul.ziyuan .ziyuan-item dd {
  width: 50%;
  float: left;border-bottom:1px solid #eee;
}
ul.ziyuan .ziyuan-item dd span {
  width: auto;
  padding-left: 16px;
  margin: 0px 16px 10px 0;
}
ul.ziyuan .ziyuan-item dd span:hover{
    cursor: pointer;
    color:#5093e1;
}
ul.ziyuan .ziyuan-item dd span.active {
  color: #d03d31;
}
/*tree*/
#treeDemo_1_a{
	text-align: center;
	text-indent: 0;
}
.list{
	margin: 0;
	padding: 0;
	list-style: none;
}
.list a{
	text-decoration: none;
    outline: none;
    color: #222;
}
/*left_main end*/

/*right section*/
.main_right{
	float: right;
}
.mainOuterWrap{position:relative;}
.boshiPic{
	position: absolute;
    right: -27px;
    top: -75px;
    display: block;
    z-index:10;
}
/*header*/
.main_right header{padding-top: 15px;font-size: 16px;color: #949494;}
.main_right header a{display: inline-block;width: 123px;height: 39px;cursor: pointer;line-height: 39px;text-align: center;}
.main_right header a.active{background: url(../img/main_tit.png)no-repeat;}  

.main_right header span{color:#6699ff;;}
/*header end*/

/*article*/
.main_right article.cont-body{position: relative;margin-top: 26px;border-radius: 5px;border: 9px solid rgba(191,225,251,.6);border-top-left-radius: 6%;border-top-right-radius: 6%;border-bottom-left-radius: 7%;border-bottom-right-radius: 7%;background:#eaf4fc;}
.mainR_ConBorder{background: rgba(235,244,252,.4);margin: 20px;border: 2px dotted #fff;border-top-left-radius: 4%;border-top-right-radius: 5%;border-bottom-left-radius: 4%;
    border-bottom-right-radius: 4%;padding-left:15px;min-height:774px;}

.cont-body header{
    overflow:hidden;

}
.cont-body header dl{
    margin: 0;
    padding: 0;
    list-style: none;
    color: #222222;
    font-size: 14px;padding-top:6px;
}
.cont-body header dl span{cursor:pointer;
    float:left;
    color: #222222;
    font-size: 14px;
    margin: 0px;
    height: 20px;
    line-height:20px;
    padding: 0 10px;
    margin:0 5px 5px;
    border-radius: 10px;
}
.cont-body header dl span.active{
    background-color: #65b0e9;
    color: #FFFFFF;
    border-bottom:0;
}
.cont-body header dl span.file_up_time,.cont-body header dl span.click_num{
    background-color: #F5F5F5;
    width: auto;
    border-radius:5px;margin-bottom:3px;
}
.cont-body header dl span.file_up_time img,.cont-body header dl span.click_num img{margin-left:3px;vertical-align:middle;}
.commSource_select{position:absolute;right:15px;top:0;width:66%;}
.cont-body header dl input{
    height: 30px;
    -moz-box-sizing: border-box;  
     -webkit-box-sizing: border-box; 
     -o-box-sizing: border-box; 
     -ms-box-sizing: border-box;
    width: 36%;
    padding-left:8px;
    border: 1px solid #ccc;
    outline: 0;background:none;
    border-radius: 5px;
    margin-right: 3%;box-sizing:border-box;
}
.cont-body header dl input#time{padding-right:4.6%;background:url(../img/calender.png) no-repeat 97% center;}
.cont-body header dl #keyworld{margin-right:0;width:60%;height:35px;border:1px solid #65b0e9;box-sizing:border-box;border-radius:17px;background:#feffff;padding-right:40px;}
.search{position:absolute;right:8px;top:1px;cursor:pointer;padding:6px;}
.cont-body header dt{
    float: left;
    height: 40px;
    line-height: 40px;
    width: 5%;
}
.cont-body header dd{
    padding-top: 10px;position:relative;
    margin: 0;
    float:left;
    width: 95%;
}
.cont-body header .orderDt{clear:both;height:20px;line-height:24px;}
.cont-body header .orderDd{line-height:20px;padding-top:2px;}
.cont-body header dd:nth-child(6){padding-top:0px;}
.commSelectChange:nth-child(3) span{margin-bottom:15px;}
 .cont-body header dt:nth-child(5){line-height:20px;font-size:1.4rem;height:20px;}
/*section*/
.cont-body section{padding-top:15px;}
.cont-body section ul{
	padding: 0;
	list-style: none;padding-right:15px;
}
.cont-body section ul li{
	padding: 0;
    margin: 0;
    margin-bottom: 24px;
	float: left;
	overflow: hidden;
	width: 27%;margin-right:9.5%;
	
}
.cont-body section ul li:nth-child(3n){margin-right:0;}
.cont-body section ul li figure{
	padding: 0;
    margin: 0;
	border: 1px solid #ddd;
}
.cont-body section ul li figcaption p.vote_num{position:absolute!important;left:50%;top:-24px;box-sizing:border-box;padding-left:28px;padding-right:10px;background:#dcf1e0 url(../img/hand.png) no-repeat 7px center;border-radius:10px;color:#50c14e;}
.cont-body section ul li figure>a{background:#f8fbfe;}
.cont-body section ul li figure .commPicWrap{background-color: #f8fbfe;padding:36px 0;display:block;cursor:pointer;border-bottom: 1px solid #e5e5e5;height:70px;}
.cont-body section ul li figure .commPicWrap>div{width:76px;height:70px;margin:0 auto;;text-align:center;position:relative;}
.cont-body section ul li figure .commPicWrap>div .jingpin{position:absolute;top:33px;left:50%;margin-left:27px;}

.cont-body section ul li figure img{
	display: block;
	margin:0 auto;
}

.cont-body section ul li figcaption{
	padding:0 4%;height:152px;
	position:relative;
}
.cont-body section ul li figcaption p{
	margin: 0;
	font-size: 12px;
	color: #818181;
	line-height: 20px;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;height:20px;
    /*width: 100%;*/
}
.cont-body section ul li figcaption p.commTitFirst{height:auto;}
.cont-body section ul li figcaption p span{white-space: nowrap;text-overflow: ellipsis;overflow:hidden;}
.down_num{text-align:right;}
.cont-body section ul li figcaption p.commTitFirst span{display:block;white-space: nowrap;text-overflow: ellipsis;overflow:hidden;}
.cont-body section ul li figcaption p.commTitFirst span.title{padding:5px 0 4px;}

.upLCxj{width:49%;}
.num_wrap{height:20px;}
.click_num,.down_num {
    float:left;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
	width: 49%;
	box-sizing:border-box;
    -moz-box-sizing:border-box;
    -ms-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    -o-box-sizing:border-box;
}
.down_num{float:right;padding-right:4px;}
.vote_num{margin: 0;font-size: 12px;color: #818181;line-height: 20px;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.cont-body section ul li figcaption p .click_num,.cont-body section ul li figcaption p .down_num,{
	
}
.down,.collection{
	cursor: pointer;
	float:left;
	height: 28px;
    font-size:14px;
    color:#333;
	line-height: 21px;
	padding-left:40px;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -ms-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    -o-box-sizing:border-box;
}
.commLine{width:100%;height:0;border-bottom:1px solid #e3e8ec;padding-top:7px;}
.downWrap{padding-top:7px;padding-right:24px;height:28px;}
.collection{
	background:url(../img/shoucang.png) no-repeat 12px top;
}
.down{
	background:url(../img/xiaz.png) no-repeat 12px top;float:right;
}
.title{
	font-size: 14px;width:100%;
	color: #222222;
	font-weight: bold;
}

/*section end*/

/*footer*/
.cont-body footer{
