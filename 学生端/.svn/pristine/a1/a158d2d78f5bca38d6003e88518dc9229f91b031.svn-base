/*日历*/
.lineTime{position: absolute;top:40%;width: 278px;height: 196px;display:none;}
.Time{
	width: 210px;
    position: absolute;
    border: 4px solid #03A428;
    border-radius: 9px;
    right: 0;
}
.ui-widget.ui-widget-content{width: 204px;}
.lineGreen{position: absolute;top: 50%;}
.ui-datepicker select.ui-datepicker-year,select.ui-datepicker-month{
    height: 24px;
    font-size: 14px;
    border: none;
    border-radius: 5px;
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    background: url("../img/se_xia.png") no-repeat 92% center #fff;
    padding-left: 4px;
    background-size: 7px 9px;
    margin-right: 5px;
    overflow: hidden;
}
select.ui-datepicker-month{margin-left: 10px;}





/*时间连*/
.chain{margin-top: 40px;}
.chain .chain-name{
	display: inline-block;
	width: 134px;
	height: 94px;
	line-height: 80px;
    padding-left: 40px;;
	margin: 30px 0 0 39px;
	font-size: 16px;
	color:#fff;
	background: url('../img/chain-name.png') no-repeat;
}
.chain .chain-grade{
	background: #fff;
	display: inline-block;
	width: 110px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	color:#5a9fbf;
	font-size: 16px;
	border-radius: 15px;
	margin-top: 50px;
    margin-left: 400px;
}
.chain .chain-select{
	display: inline-block;
	width: 110px;
	height: 36px;
	color:#5a9fbf;
	font-size: 16px;
	border-radius: 15px;
	outline: none;
	border:none;
	padding-left: 28px;
	appearance:none;  
  	-moz-appearance:none;  
  	-webkit-appearance:none;
  	background: url('../img/chain-la.png')  no-repeat scroll 70px center #fff;
  	 margin-top: 50px;
    margin-left: 98px;
}
.chain .chain-time{
	background: #fff;
	display: inline-block;
	width: 150px;
	height: 36px;
	text-align: center;
	line-height: 36px;
	color:#5a9fbf;
	font-size: 16px;
	border-radius: 15px;
	margin-right: 20px;
	margin-top: 50px;
}
.chain .chain-con{
	width: 1200px;
	height: 660px;
	background: #fff;
	background: rgba(255,255,255,0.2);
	border-radius: 10px;
	padding: 10px;
	box-sizing: border-box;

}
.chain .Line{
	height: 670px;
	overflow-y: auto;
	border:2px dashed #3399cc;
	border-radius: 10px;
}
.chain .Line li{float: left;}
.chain .Line li:nth-of-type(odd){margin-right: 100px;margin-left: 20px;}
.chain .Line li span{
	display: inline-block;
	width: 84px;
	height: 84px;
	text-align: center;
	line-height: 84px;
	font-size: 14px;
	margin: 10px 0 0 10px;
}
.chain .Line li em{
	display: inline-block;
    font-style: normal;
    border-radius: 10px;
