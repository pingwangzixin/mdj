html,body {
    width: 100%;
    height: 100%;
}
.body{
	background-position:center 0;
	background-repeat: no-repeat;
}
.main{
	width: 1200px;
	height:1000px;
	margin: 0 auto;
	position: relative;
}
.cursor{cursor: pointer;cursor: hand;}

/*头部*/
.head>div{
	float: left;
	width: 33%;
	height: 38px;
	line-height: 38px;
	margin-top: 10px;
}
.head>div img.logo{
	display: block;
	width: 228px;
	height: 38px;
	margin: 0 auto;
	text-align: center;
}
.head>div img.dangan{
	display: block;
	margin: 0 auto;
	text-align: center;
	margin-top: 12px;
}
.head>div.person{text-align: right;margin-top: 20px;}
.head>div.person img{margin-top: 10px;margin-right: 6px;}
.head>div.person i{margin-right:30%;font-size: 16px;color:#fff;font-style: normal;}



