
body{min-width:1200px;}
a{
	text-decoration: none;
}
.yun{width:80%;margin:100px auto;}
.main .main_left{width: 12%;height: 100%;min-height: 720px;margin-left: 2.5%;float:left;}
.mainL_border{padding-top:46px;
    border: 7px solid #bddffa;
    border-radius: 70px 70px 70px 50px;
    margin-top: 27px;
    background-color: #eaf4fc;}
	/*ziyuan   */
ul.ziyuan,.ziyuan dl,.ziyuan dt,.ziyuan dd{
  list-style: none;
  margin: 0;
  padding: 0;
  text-align: center;
}
ul.ziyuan li {
  height: 41px;
  line-height:41px;
  border-top: 0px;
  position: relative;color:#666;cursor:pointer;
}
ul.ziyuan li.active,ul.ziyuan li:hover{background:#b8d9f3;}
ul.ziyuan li.active{font-weight:bold;}
ul.ziyuan li span{font-size:14px;}
/*right section*/
.main_right{
	float: right;
}
.mainOuterWrap{position:relative;}
/*小博士*/
.boshiPic{
	position: absolute;
    right: -27px;
    top: -75px;
    display: block;
    z-index:10;
    cursor: pointer;
}
.main_right header{padding-top: 15px;font-size: 16px;color: #949494;}
.main_right header a{display: inline-block;width: 123px;height: 39px;cursor: pointer;line-height: 39px;text-align: center;}
.main_right header a.active{background: url(../img/main_tit.png)no-repeat;}  

.main_right header span{color:#6699ff;;}

/*article*/
.main_right article.cont-body{position: relative;margin-top: 26px;border-radius: 5px;border: 9px solid rgba(191,225,251,.6);border-top-left-radius: 6%;border-top-right-radius: 6%;border-bottom-left-radius: 7%;border-bottom-right-radius: 7%;background:#eaf4fc;}
.mainR_ConBorder{background: rgba(235,244,252,.4);margin: 20px;border: 2px dotted #fff;border-top-left-radius: 4%;border-top-right-radius: 5%;border-bottom-left-radius: 4%;
    border-bottom-right-radius: 4%;padding:0 15px;min-height:774px;}
/*内容部分=================================================*/
.mainR_ConBorder .out{width:76px;height:26px;line-height:26px;text-align:center;font-size:14px;background:#bdd8f4;color:#5093e1;border-radius:11px;margin:30px 30px 0 0px;cursor:pointer;}
.mainR_ConBorder .detail{margin-top:72px;overflow:hidden;}
.mainR_ConBorder .detail li{width:31%;float:left;border:1px solid #92c7f0;padding:16px 0 16px 10px;box-sizing:border-box;-moz-box-sizing:border-box; border-radius:5px;}
.mainR_ConBorder .yan{width:50px;height:50px;text-align:center;line-height:50px;background:#a0c6ef;color:#5093e1;font-size:20px;border-radius:5px;}
.mainR_ConBorder .yan.girl{color:#f65177;background:#f0a6bc;}
.mainR_ConBorder .person{margin-left:7%;color:#222;font-size:14px;line-height:20px;}
.mainR_ConBorder .person>div:nth-child(2){padding-top:8px;}
.mainR_ConBorder .person .jibie{font-size:16px;margin-right:10%;}
.mainR_ConBorder .person .shuxian{margin:0 7px;}

/*footer分页================================================*/
.cont-body footer{
	text-align: center;margin-bottom:20px;
}
.cont-body footer span{
	font-size: 16px;
	cursor: pointer;
	display: inline-block;
	margin: 0 2px;
	min-width: 14px;
	height: 22px;
	padding: 0 4px;
    line-height: 22px;
    color: #5093E1;vertical-align:middle;
    border-radius: 5px;
}
.cont-body footer span.active{
	background-color: #5093E1;
	color: #FFFFFF;
	border-radius: 5px;
}
.yun_big {
    width: 100%;
    text-align: center;
    margin: 140px auto 40px;
}
/*弹窗样式========================*/
.delSureDel {display:none;}
.delSureDel .con{padding-top:20px;width:485px;height:265px;border-radius:50px;box-sizing:border-box;padding:18px;background:#fff;border:7px solid #9bbed8;}
.delSureDel .delTishi{color:#5093e1;font-size:1.6rem;position:absolute;left:26px;top:20px;}
.delSureDel .delClose{position:absolute;padding:12px;top:-7px;right:-13px;margin:0!important;cursor: pointer;}
.delSureDel .tijiao{width:276px;overflow:hidden;margin: 56px auto 0;padding-right:0;}
.delSureDel img{display:block;padding:34px 0 10px;margin:0 auto;}
.delSureDel .tijiao div{float:left;width:108px;height:30px;line-height:30px;text-align:center;border-radius:7px;cursor: pointer;font-size:1.4rem;}
.delSureDel .tijiao .upsuresc{float:right;}
.delSureDel .upsuresc{color:#fff;background:#00cc99;}
.delSureDel .upqxsc{background:#ccc;color:#fff;}
.delSureDel .text-text{padding-top:64px;display:block;font-size:1.6rem;}
.uploadzy .upsuresc,.weikeupload .upsuresc {
    color: #fff;float:right!important;
    background: #00cc99;
}
.zhezhao {
    background: rgba(0,0,0,.3);
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 10;
    display: none;
}


/*&&&&&&&&&&&&&&&&&&&&社团详情页样式&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&*/
.contentWrap{position:relative;min-height:112px;padding:36px 0;}
.contentWrap.border{border-bottom:2px solid #fff;}
.conLeft{color:#222;font-size:16px;width:112px;height:112px;background:url(../img/shetuanBg.png) no-repeat center;position:absolute;top:50%;margin-top:-56px;line-height:112px;text-align:center;}
.conRight{padding-left:148px;line-height:28px;color:#555;font-size:14px;    word-break: break-word;word-wrap: break-word;}
.conRight img{float:left;margin:0 14px 14px 0;}




