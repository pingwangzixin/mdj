html,body {
    width: 100%;
    height: 100%;
    background-color: #FEE78A;
    box-sizing: border-box;
}
.main{
	width: 100%; 
	height: 100%;
	/*background: url("../img/high-bg.jpg");*/  /*高中*/
	background: url("../img/middle-bg.jpg");      /*初中*/
	background-position: center center;
	background-repeat: no-repeat;
	background-size:100% 100%;
	min-width: 1200px;
	min-height: 800px;
	box-sizing: border-box;
}
.page{ 
	width: 1200px;
	min-height: 800px; 
	margin: 0 auto;
	background-repeat: no-repeat; 
	background-position: center center;
	position: relative;
}
.cursor{cursor: pointer;cursor: hand;}
/*头部*/
.head>div{
	float: left;
	width: 33%;
	height: 38px;
	line-height: 38px;
	margin-top: 10px;
}
.head>div img.logo{
	display: block;
	width: 228px;
	height: 38px;
	margin: 0 auto;
	text-align: center;
}
.head>div img.dangan{
	display: block;
	margin: 0 auto;
	text-align: center;
	margin-top: 12px;
}
.head>div.person{text-align: right;}
.head>div.person img{margin-top: 10px;margin-right: 6px;}
.head>div.person i{margin-right:30%;font-size: 16px;color:#fff;font-style: normal;}



