.animationImg .star_left{position: absolute;left:60px;top:220px; -webkit-animation: fadeStar 1.5s 0.4s infinite;-moz-animation: fadeStar 1.5s 0.4s infinite;animation: fadeStar 1.5s 0.4s infinite;}
.animationImg .star_right{position: absolute;left:200px;top:35px;-webkit-animation: fadeStar 1s 1s infinite;-moz-animation: fadeStar 1s 1s infinite;animation: fadeStar 1s 1s infinite;}
.animationImg .star_center{position: absolute;right:63px;top:132px;-webkit-animation: fadeStar 2s 0.8s infinite;-moz-animation: fadeStar 2s 0.8s infinite;animation: fadeStar 2s 0.8s infinite;}
@-webkit-keyframes count{
	0%{-webkit-transform: scale(0);}
	70%{-webkit-transform: scale(1.3);}
	85%{-webkit-transform: scale(0.8);}
	100%{-webkit-transform: scale(1);}
}
@-moz-keyframes count{
	0%{transform: scale(0);}
	70%{transform: scale(1.3);}
	85%{transform: scale(0.8);}
	100%{transform: scale(1);}
}
@-webkit-keyframes fadeStar{
	0%{opacity: 1;}
	50%{opacity: 0;}
	100%{opacity: 1;}
}
@-moz-keyframes fadeStar{
	0%{opacity: 1;}
	50%{opacity: 0;}
	100%{opacity: 1;}
}
.countNotice .hide{opacity: 1 !important;}
