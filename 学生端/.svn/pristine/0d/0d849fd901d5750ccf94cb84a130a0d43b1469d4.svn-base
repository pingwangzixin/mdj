body {
    color: #212121;
    position: relative;
    font-family: Microsoft YaHei, Arial, sans-serif;overflow-x:hidden;
    min-width:1200px;
    overflow-x: auto;
}
.main{box-sizing:border-box;padding-bottom:54px;}
.main .main_left{box-sizing:border-box;}
.main .main_right{float:right;padding-top:20px;}
.con_wrap{position:relative;}
.main_right .con {
    box-shadow: 0 0 10px #ccc;
    width: 94%;
    background-color: #eaf4fc;
    min-height: 830px;
    padding: 16px 20px;
    border:7px solid #bfe1fa;border-radius:6% 6% 7% 7%;z-index:2;
}

/* */
.con {
    position: relative;
}
#xiaoboshi{
	position: absolute;
    right: 8px;
    top: -64px;
    width: 100px;
    z-index: 11;
}
.con_border{
	width:100%;
	height:100%;
	box-sizing:border-box;
	border: 2px dotted #fff;
	border-top-left-radius: 4%;
    border-top-right-radius: 5%;
    border-bottom-left-radius: 4%;
    border-bottom-right-radius: 4%;
	padding:16px 5px 22px;
	min-height:830px;
	background: url(../img/yun_big.png) no-repeat center 75%;
}
.con .path-navigation {
    height: 38px;
    padding-left: 10px;
 
}
.con .path-navigation ul,.con .path-navigation ul li {
    float: left;
    vertical-align: middle;
  
}
.con .path-navigation ul li span {
    font-size: 1.4rem;
    color: #212121;  line-height: 40px;
}
.con .path-navigation ul li span.xie {
    margin: 0 5px;
}
.con .path-navigation ul li img {
    margin: -4px 20px 0 20px;
    vertical-align: middle;
}
.con .path-navigation .path-navigation-btn {
    float: right;
    margin-right: 20px;
}
.con .path-navigation .path-navigation-btn li {
    width: 104px;
    border: 1px solid #5EA6D9;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 10px;
    line-height: 32px;
    margin: 13px 0 0 20px;
}
.con .path-navigation .path-navigation-btn li a {
    display: block;
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 1.4rem;
    color: #5EA6D9;
}
.con .path-right {
    display: block;
    position: relative;
    float: right;
    height:38px;
    line-height: 38px;
    margin-right: 15px;
    overflow: hidden;
}
.con .path-right .sousuo {
	float:left;
	display: block;
    width: 20px;
    height: 20px;
    margin-top:8px;
    cursor: pointer;
    margin-left:-40px;
}
.con .path-right input {
    float:left;
    width: 270px;
    height: 36px;
    background: #F5F5F5;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
    border: 1px solid #4F93E0;
    outline: none;
    padding-left: 30px;
    padding-right:50px;
    font-size: 14px;
}
/*涓婚鍐呭 鍙充晶  table鏍忓垏鎹�*/
.investment_f {
    margin:0 auto;
}
.investment_f .investment_title {
    height:48px;
    width:100%;
    overflow:hidden;
    display: inline-block;
    border-left:1px solid #fff;
    padding-top:1px;
}
.investment_title div {
    width:25%;
    height:48px;
    float:left;
    display:inline;
    font-size:1.6rem;
    text-align:center;
    line-height:48px;
    cursor:pointer;
    background: #ebf4fc;
   border:1px solid #fff;border-left:0 none;box-sizing:border-box;

}
.investment_title div span {
    font-size: 1.4rem;
}
.investment_title a,.investment_title a:link,.investment_title a:visited,.investment_title a:hover {
    color:#555;
    width:131px;
    height:44px;
    display:inline-block;
}
.investment_title div.on a,.investment_title div.on a:link,.investment_title div.on a:visited,.investment_title div.on a:hover{
    color:#f70;
}
.investment_title div.on {
    background: #fff;
    color: #4F93E0; 
    border-top: 2px solid #a4dffd;
    margin-top:-1px;
}
.investment_con {
    clear:both;margin-top: -4px;
}
.investment_con tr{border-left:1px solid #fff;}
