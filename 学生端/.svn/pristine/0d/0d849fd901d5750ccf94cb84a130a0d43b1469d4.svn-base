.investment_btn {
    width: 100%;
    overflow:hidden;
    float: right;
}
.investment_btn dl {
    width: 76%;
    padding-left:40px;
    box-sizing:border-box;
    float: left;
    margin-top: 13px;
    line-height:21px;
}
.investment_btn dl dt {
    float: left;
    margin: 0 5px 0 10px;
    font-size:1.4rem;
    margin-left:-40px;
}
.investment_btn dl dd {
    padding: 0 10px;
    height: 21px;
    float: left;
    cursor: pointer;
    font-size:1.4rem;
    margin:0 2px 15px;
}
.investment_btn dl dd.active {
    border-radius: 15px;
    background: #65b0e9;
    color: #fff;
}
.investment_btn ul {
    width: 24%;
    float: right;
    font-size:1.4rem;
}
.investment_btn ul li {
	min-width:70px;
	width:32%;
	box-sizing:border-box;
	padding:0 3px;
    float: left;
    text-align: center;
    height: 44px;
    color: #65b0e9;
    line-height: 44px;
    cursor: pointer;
    font-size:1.2rem;
}
.investment_btn ul li#download {
    color: #19e199;vertical-align:middle;
}
.investment_btn ul li#delete {
    color: #ff3333;
}
.investment_btn ul li#download span{
	padding-left:20px;
	background: url(../img/gerenLoad.png) no-repeat left center;
}
.investment_btn ul li#delete span {
    padding-left:20px;
	background: url(../img/delete_red.png) no-repeat left center;
}

