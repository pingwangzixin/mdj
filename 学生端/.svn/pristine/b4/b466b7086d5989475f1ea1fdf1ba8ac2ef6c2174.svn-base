	<script type="text/javascript">
		$(document).ready(function(){
			$('.head .list li a').on('click',function(){
				$(this).addClass('active').parent('li').siblings('li').find('a').removeClass('active');
			});
		});
		
		//移入显示消息列表
		var timer = null;
		var objF = $('.remind_list ul');
		$('.remind').on('mouseover',function (){
			clearTimeout(timer);
			$('.remind_list').show();
			! objF.children().length ? $('.remind_list>i').show() : $('.remind_list>i').hide();
		}).on('mouseout',function (){
			timer=setTimeout(function (){
				$('.remind_list').hide();
			},300);
		});
		
		
		//点击列表隐藏列表
		$(document).on('click','.remind_list ul li',function (){
			$('.remind_list').hide();
		});
		
		
		//消息列表移入显示详情
		$(document).on('mouseover','.remind_list ul li',function (){
			$(this).attr({'title':$(this).text()});
		});
		
