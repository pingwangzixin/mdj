<!--页面js-->
		<script>
			$(function(){
				//学科hover的弹出层
				$(".xuekejs").on("mouseover mouseout",function(event){
					if(event.type == "mouseover"){
						$(this).parent().find(".contitle").show()
					}else if(event.type == "mouseout"){
						$(this).parent().find(".contitle").hide()
					}
				})

				//快速评价下拉选项弹出
				$(".kspj").each(function(index){
					$(this).on("mouseenter",function(){
						$(".selectpy").eq(index).show()
					})
					$(".pingyu").on("mouseleave",function(){
						$(".selectpy").eq(index).hide()
					})
				})

				//选择修改快速评价的下拉内容
				$(".selectpy li").on("click",function(){
					var pingyu= $(this).html();
					$(this).parents(".selectpy").siblings("textarea").html(pingyu)
				})
				
				$(".hpgb,.hp_gbbtn").on("click",function(){
					$(".hupingtc").hide();
				})
				
				$(".hp_table").on("click",function(){
					$(".hupingtc").show()
				})
			})
		</script>
